<?xml version="1.0" encoding="UTF-8"?><vxml version="2.0" application="vh1Root.vxml" xmlns="http://www.w3.org/2001/vxml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">	<meta name="vh1_end.vxml" content=""/>	<meta name="Author" content=""/>	<meta name="Date" content=""/>	<meta name="Description" content=""/>	<meta name="Support" content=""/>	<form id="EndLogin">		<block>			<log>				Try n Buy entry				<value expr="ispressedsubdtmf"/>			</log>			<log>				program_name				<value expr="program_name"/>			</log>			<if cond="ispressedsubdtmf!='YES'">				<!--<log>Try n Buy entry</log>
				<log>Try n Buy url:<value expr="'http://'+IP+'/'+SERVLETPATH+'/'+dbservlet+'?PROCEDURE=JBOX_SUB_TnB&amp;INTOKEN=8&amp;OUTTOKEN=0&amp;INPARAM[0='+ANI+'&amp;INPARAM[1='+lang+'&amp;INPARAM[2=TIVR&amp;INPARAM[3='+DNIS+'&amp;INPARAM[4=2&amp;INPARAM[5=1507&amp;INPARAM[6=47&amp;INPARAM[7='+circle"/></log>

				
	  			<script srcexpr="'http://'+IP+'/'+SERVLETPATH+'/'+dbservlet+'?PROCEDURE=JBOX_SUB_TnB&amp;INTOKEN=8&amp;OUTTOKEN=0&amp;INPARAM[0='+ANI+'&amp;INPARAM[1='+lang+'&amp;INPARAM[2=TIVR&amp;INPARAM[3='+DNIS+'&amp;INPARAM[4=2&amp;INPARAM[5=1507&amp;INPARAM[6=47&amp;INPARAM[7='+circle"/>	-->			</if>			<if cond="timerflag=='1'">				<assign name="current_balance" expr="start_time()"/>				<log>					call end time:					<value expr="current_balance"/>				</log>				<log>					call start time balance:					<value expr="initial_balance"/>				</log>				<script>					total_used_bal=CheckUserBalance(initial_balance,current_balance);
								     		curbalance=balance-total_used_bal;				</script>				<log expr="'@@@@@@ used balance is '+total_used_bal"/>				<log expr="'@@@@@@ left balance is '+curbalance"/>				<script>					curbalance=Math.floor(curbalance/60);				</script>				<if cond="curbalance &lt; '0'">					<script>						curbalance=0;					</script>				</if>			</if>			<assign name="C_End" expr="start_time()"/>			<log>				song :				<value expr="song"/>			</log>			<if cond="song_listen=='START'">				<assign name="song_listen" expr="'END'"/>				<assign name="compare_time" expr="time_duration(song_begin)"/>				<assign name="ContentTime" expr="AddContentTime(compare_time,ContentTime)"/>				<assign name="contentlog" expr="contentlog+hash+song+hash+playconfig+hash+compare_time+hash"/>			</if>			<if cond="playconfig=='01'||playconfig=='02'||playconfig=='03'||playconfig=='04'">				<script srcexpr="'http://'+IP+'/hungama/vh_dbinteraction?PROCEDURE=VH1_USERPROFILE&amp;INTOKEN=3&amp;OUTTOKEN=1&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+playconfig+'&amp;INPARAM[2]=SET'"/>			</if>			<log expr="'CallTime@@@@@@@@@@@@@@@@@@@@ =' + CallTime"/>			<script>				total_used_bal=rim(CallTime);
			  	balance=balance-total_used_bal;			</script>			<log expr="'balance@@@@@@@@@@@@@@@@@@@@ =' + balance"/>			<script>				balance=Math.floor(balance/60);			</script>			<script>				total_used_bal=Math.floor(total_used_bal/60);			</script>			<!--
				<assign name="total_used_bal" expr="time_duration(C_Start)"/>
				<script> total_used_bal=(Math.floor(total_used_bal/60))+1; </script>  	
		 		-->			<log>				Pending minute is:				<value expr="balance"/>			</log>			<log>				curbalance is:				<value expr="curbalance"/>			</log>			<log>				totalusedbal is:				<value expr="total_used_bal"/>			</log>			<if cond="DNIS=='5584112' &amp;&amp; (circle=='nes' || circle=='asm')">				<script srcexpr="'http://'+IP+'/hungama/'+dbservlet+'?PROCEDURE=NIGHTPACK_UPDATETIME&amp;INTOKEN=4&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+curbalance+'&amp;INPARAM[2]=5584112&amp;INPARAM[3]='+timerflag"/>			<else/>				<script srcexpr="'http://'+IP+'/hungama/'+dbservlet+'?PROCEDURE=JBOX_UPDATETIME&amp;INTOKEN=4&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+total_used_bal+'&amp;INPARAM[2]=55841&amp;INPARAM[3]='+timerflag"/>			</if>			<if cond="sub_flag=='true'">				<script srcexpr="'http://'+IP+'/hungama/Voda_Concentlog?ANI='+ANI+'&amp;circle='+circle+'&amp;service=1307&amp;SC='+DNIS+'&amp;firstconsent='+firstconsent+'&amp;firstconsenttime='+firstconsenttime+'&amp;secondconsent='+secondconsent+'&amp;secondconsenttime='+secondconsenttime+'&amp;redirectdnis='+DC_DNIS"/>			</if>			<script srcexpr="'http://'+IP+'/hungama/exit_log?starttime='+C_Start+'&amp;endtime='+C_End+'&amp;ani='+ANI+'&amp;dnis='+DNIS+'&amp;realdnis='+realDNIS+hash+program_name+hash+modesub+hash+pricepoint+'&amp;contenttime='+ContentTime+'&amp;circle='+circle+'&amp;operator='+operator+'&amp;service=vh1&amp;subflag='+usr_status+'&amp;contentlog='+contentlog"/>			<throw event="connection.disconnect.hangup"/>		</block>		<catch event="error.badfetch">			<log>				Inside Error BadCatch catch  End2222			</log>			<exit/>		</catch>		<catch event="telephone.disconnect.hangup">			<log>				Inside Telephone Disconnect Hangup catch  End3333			</log>			<exit/>		</catch>		<catch event="telephone.disconnect">			<log>				Inside Telephonet Disconnected catch  End4444			</log>			<exit/>		</catch>		<catch event="connection.disconnect.hangup">			<log>				Inside connection.disconnect.hangup catch  End5555			</log>			<exit/>		</catch>	</form></vxml>