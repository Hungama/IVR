<?xml version="1.0" encoding="UTF-8"?><vxml version="2.0" application="jyotish_root1.vxml" xmlns="http://www.w3.org/2001/vxml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">	<meta name="PlaySunSign.vxml" content=""/>	<meta name="Author" content=""/>	<meta name="Date" content=""/>	<meta name="Description" content=""/>	<meta name="Support" content=""/>	<var name="RepeatName" expr="'selsunsign'"/>	<script>		var userselectsun= new Array();	</script>	<error>		<goto next="end.vxml"/>	</error>	<form id="checkMinStatus">		<block>			<goto next="#PlaySunSign"/>		</block>	</form>	<form id="PlaySunSign">		<block>			<log>				Play Sun Sign Section			</log>			<assign name="counter" expr="1"/>			<assign name="userselectsunsign[0]" expr="select_sunsign1"/>			<assign name="userselectsunsign[1]" expr="'select_sunsign1'"/>			<assign name="userselectsunsign[2]" expr="'select_sunsign2'"/>			<assign name="userselectsunsign[3]" expr="'select_sunsign3'"/>			<assign name="userselectsunsign[4]" expr="'select_sunsign4'"/>			<assign name="userselectsunsign[5]" expr="'select_sunsign5'"/>			<assign name="userselectsunsign[6]" expr="'select_sunsign6'"/>			<assign name="userselectsunsign[7]" expr="'select_sunsign7'"/>			<assign name="userselectsunsign[8]" expr="'select_sunsign8'"/>			<assign name="userselectsunsign[9]" expr="'select_sunsign9'"/>			<assign name="userselectsunsign[10]" expr="'select_sunsign10'"/>			<assign name="userselectsunsign[11]" expr="'select_sunsign11'"/>			<assign name="userselectsunsign[12]" expr="'select_sunsign12'"/>			<clear namelist="selsunsign"/>		</block>		<field name="selsunsign" type="digits?length=1">			<grammar src="all.grxml" mode="dtmf"/>			<catch event="error.badfetch" count="1">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/invalidoption.wav'"/>				</prompt>				<assign name="counter" expr="7"/>				<goto nextitem="selsunsign"/>			</catch>			<catch event="error.badfetch" count="2">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/invalidoption.wav'"/>				</prompt>				<assign name="counter" expr="7"/>				<goto nextitem="selsunsign"/>			</catch>			<catch event="error.badfetch" count="3">				<exit/>			</catch>			<catch event="noinput" count="1">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/nodtmf.wav'"/>				</prompt>				<assign name="counter" expr="1"/>				<goto nextitem="selsunsign"/>			</catch>			<catch event="noinput" count="2">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/nodtmf.wav'"/>				</prompt>				<assign name="counter" expr="1"/>				<goto nextitem="selsunsign"/>			</catch>			<catch event="noinput" count="3">				<exit/>			</catch>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+userselectsunsign[counter]+'.wav'"/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+ 'press_dtmf1.wav' "/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+userselectsunsign[counter+1]+'.wav'"/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+ 'press_dtmf2.wav' "/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+userselectsunsign[counter+2]+'.wav'"/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+ 'press_dtmf3.wav' "/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+userselectsunsign[counter+3]+'.wav'"/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+ 'press_dtmf4.wav' "/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+userselectsunsign[counter+4]+'.wav'"/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+ 'press_dtmf5.wav' "/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+userselectsunsign[counter+5]+'.wav'"/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/'+ 'press_dtmf6.wav' "/>			<prompt timeout="3s" cond="counter &lt;=6">				<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/sign_other_option.wav'"/>				<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/press_dtmf7.wav'"/>			</prompt>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/select_acmanager.wav'"/>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/press_dtmf9.wav'"/>			<filled>				<if cond="selsunsign == '1'">					<assign name="gSunSign" expr="counter"/>					<log>						selsunsign is						<value expr="gSunSign"/>					</log>					<assign name="updatesunsign" expr="sunsign+gSunSign"/>					<assign name="selectedsunsign" expr="userselectsunsign[counter]"/>					<elseif cond="selsunsign == '2'"/>					<assign name="gSunSign" expr="counter+1"/>					<assign name="updatesunsign" expr="sunsign+gSunSign"/>					<assign name="selectedsunsign" expr="userselectsunsign[counter+1]"/>					<log>						selsunsign is						<value expr="gSunSign"/>					</log>					<elseif cond="selsunsign == '3'"/>					<assign name="gSunSign" expr="counter+2"/>					<assign name="updatesunsign" expr="sunsign+gSunSign"/>					<assign name="selectedsunsign" expr="userselectsunsign[counter+2]"/>					<log>						selsunsign is						<value expr="gSunSign"/>					</log>					<elseif cond="selsunsign == '4'"/>					<assign name="gSunSign" expr="counter+3"/>					<assign name="updatesunsign" expr="sunsign+gSunSign"/>					<assign name="selectedsunsign" expr="userselectsunsign[counter+3]"/>					<log>						selsunsign is						<value expr="gSunSign"/>					</log>					<elseif cond="selsunsign == '5'"/>					<assign name="gSunSign" expr="counter+4"/>					<assign name="updatesunsign" expr="sunsign+gSunSign"/>					<assign name="selectedsunsign" expr="userselectsunsign[counter+4]"/>					<log>						selsunsign is						<value expr="gSunSign"/>					</log>					<elseif cond="selsunsign == '6'"/>					<assign name="gSunSign" expr="counter+5"/>					<assign name="updatesunsign" expr="sunsign+gSunSign"/>					<assign name="selectedsunsign" expr="userselectsunsign[counter+5]"/>					<log>						selsunsign is						<value expr="gSunSign"/>					</log>					<elseif cond="selsunsign == '7'"/>					<log>						selsunsign is						<value expr="gSunSign"/>					</log>					<assign name="counter" expr="counter + 6"/>					<log>						More Sun Sign					</log>					<goto nextitem="selsunsign"/>					<elseif cond="selsunsign == '9'"/>					<log>						selsunsign is						<value expr="gSunSign"/>					</log>					<goto next="AccountManager.vxml" fetchhint="prefetch" fetchtimeout="10s"/>					<else/>					<throw event="nomatch"/>				</if>				<goto next="#confirmation"/>			</filled>			<nomatch count="1">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/invalidoption.wav'"/>				</prompt>				<goto nextitem="selsunsign"/>			</nomatch>			<nomatch count="2">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/invalidoption.wav'"/>				</prompt>				<goto nextitem="selsunsign"/>			</nomatch>			<nomatch count="3">				<exit/>			</nomatch>		</field>	</form>	<form id="confirmation">		<block>			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/welcome_sunsign'+gSunSign+'.wav'"/>		</block>		<field name="welsunsign" type="digits?length=1">			<catch event="noinput">				<goto nextitem="keypadnavigation"/>			</catch>			<grammar src="all.grxml" mode="dtmf"/>			<filled>				<if cond="welsunsign == '8'">					<goto next="#PlaySunSign"/>					<else/>					<goto nextitem="keypadnavigation"/>				</if>			</filled>		</field>		<field name="keypadnavigation" type="digits?length=1">			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/keypad.wav'"/>			<catch event="noinput">				<assign name="filetoplay" expr="CurrentDate()"/>				<goto nextitem="contentplay"/>			</catch>			<filled>				<if cond="keypadnavigation== '7'">					<goto nextitem="playkeypad"/>					<else/>					<assign name="filetoplay" expr="CurrentDate()"/>					<goto nextitem="contentplay"/>				</if>			</filled>		</field>		<block name="playfeed">			<assign name="filetoplay" expr="CurrentDate()"/>			<goto nextitem="confirm"/>		</block>		<field name="confirm" type="digits?length=1">			<catch event="noinput" count="1">				<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/nodtmf.wav'"/>				<goto nextitem="contentplay"/>			</catch>			<catch event="noinput" count="2">				<goto next="#PlaySunSign"/>			</catch>			<catch event="nomatch" count="1">				<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/invalidoption.wav'"/>				<goto nextitem="contentplay"/>			</catch>			<catch event="nomatch" count="2">				<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/invalidoption.wav'"/>				<goto next="#PlaySunSign"/>			</catch>			<catch event="error.badfetch">				<goto next="#PlaySunSign"/>			</catch>			<prompt timeout="5s">				<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/day_options.wav'"/>			</prompt>			<log>				Current Date is:				<value expr="CurrentDate()"/>			</log>			<grammar src="all.grxml" mode="dtmf"/>			<filled>				<if cond="confirm == '1'">					<log>						Selected dtmf:1					</log>					<assign name="filetoplay" expr="PrevDate()"/>					<goto nextitem="contentplay"/>					<elseif cond="confirm == '2'"/>					<assign name="filetoplay" expr="NextDate()"/>					<goto nextitem="contentplay"/>					<elseif cond="confirm == '7'"/>					<goto nextitem="playkeypad"/>					<elseif cond="confirm == '8'"/>					<goto next="#PlaySunSign"/>					<elseif cond="confirm == '9'"/>					<goto next="AccountManager.vxml"/>					<else/>					<goto nextitem="contentplay"/>				</if>			</filled>			<catch event="error.badfetch">				<goto next="#PlaySunSign"/>			</catch>		</field>		<block name="contentplay">			<catch event="error.badfetch">				<goto next="#PlaySunSign"/>			</catch>			<script>				total_used_bal=chkBalenceMinute(start_BalenceMinute);             			curbalance=balance-total_used_bal;			</script>			<log>				current Balance:				<value expr="curbalance"/>			</log>			<log>				Total Balance:				<value expr="total_used_bal"/>			</log>			<if cond="curbalance &lt; 0">				<!--<goto next="TopUp.vxml"/>-->			</if>			<log>				Calling Update minutes url			</log>			<log>				Inparam 1 : sunsign				<value expr="gSunSign+'_'+filetoplay"/>			</log>			<log>				Inparam 2 : select_sunsign				<value expr="gSunSign"/>			</log>			<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=Jyotish_UpdateMinutes&amp;INTOKEN=3&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]=sunsign'+gSunSign + '&amp;INPARAM[2]=sunsign'+gSunSign"/>			<goto nextitem="contentconfirm"/>		</block>		<field name="contentconfirm" type="digits?length=1">			<prompt timeout="1s">				<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/sunsign'+gSunSign+'_'+filetoplay+'.wav'"/>			</prompt>			<grammar src="all.grxml" mode="dtmf"/>			<filled>				<if cond="contentconfirm== '1'">					<log>						Selected dtmf:1					</log>					<assign name="filetoplay" expr="PrevDate()"/>					<goto nextitem="contentplay"/>					<elseif cond="contentconfirm== '2'"/>					<assign name="filetoplay" expr="NextDate()"/>					<goto nextitem="contentplay"/>					<elseif cond="contentconfirm== '7'"/>					<goto nextitem="playkeypad"/>					<elseif cond="contentconfirm== '8'"/>					<goto next="#PlaySunSign"/>					<elseif cond="contentconfirm== '9'"/>					<goto next="AccountManager.vxml"/>					<else/>					<goto nextitem="playfeed"/>				</if>			</filled>			<catch event="nomatch noinput" count="1">				<goto nextitem="playfeed"/>			</catch>			<catch event="nomatch noinput" count="2">				<goto next="#PlaySunSign"/>			</catch>			<catch event="error.badfetch">				<goto nextitem="playfeed"/>			</catch>		</field>		<block name="chkMin">			<script>				total_used_bal=chkBalenceMinute(start_BalenceMinute);             			curbalance=balance-total_used_bal;			</script>			<if cond="curbalance &lt;= '0'">				<assign name="backmovepage" expr="'PlaySunSign.vxml'"/>				<!--<goto next="TopUp.vxml"/>-->				<else/>				<goto nextitem="contentplay"/>			</if>			<goto next="#PlaySunSign"/>		</block>		<block name="playkeypad">			<log>				Keypad options			</log>			<goto nextitem="confirmkeypad"/>		</block>		<field name="confirmkeypad" type="digits?length=1">			<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/keypad_options.wav'"/>			<grammar src="all.grxml" mode="dtmf"/>			<catch event="noinput" count="1">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/nodtmf.wav'"/>				</prompt>				<goto nextitem="playkeypad"/>			</catch>			<catch event="noinput" count="2">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/nodtmf.wav'"/>				</prompt>				<goto nextitem="playkeypad"/>			</catch>			<catch event="noinput" count="3">				<exit/>			</catch>			<filled>				<if cond="confirmkeypad== '1'">					<goto nextitem="contentplay"/>					<elseif cond="confirmkeypad== '3'"/>					<goto nextitem="contentplay"/>					<elseif cond="confirmkeypad== '8'"/>					<goto next="#PlaySunSign"/>					<elseif cond="confirmkeypad== '9'"/>					<goto next="AccountManager.vxml"/>					<else/>					<throw event="nomatch"/>				</if>			</filled>			<nomatch count="1">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/invalidoption.wav'"/>				</prompt>				<goto nextitem="confirmkeypad"/>			</nomatch>			<nomatch count="2">				<prompt timeout="5s">					<audio expr="promptpath+'/'+LANG_DESCRIPTION+'/invalidoption.wav'"/>				</prompt>				<goto nextitem="confirmkeypad"/>			</nomatch>			<nomatch count="3">				<exit/>			</nomatch>		</field>	</form>	<form id="checkbalance">		<block>			<script>				total_used_bal=chkBalenceMinute(start_BalenceMinute);             			curbalance=balance-total_used_bal;			</script>			<if cond="curbalance &lt;= '0'">				<assign name="backmovepage" expr="'PlaySunSign.vxml'"/>				<!--<goto next="TopUp.vxml"/>-->				<else/>				<goto next="#PlaySunSign"/>			</if>			<goto next="#PlaySunSign"/>		</block>	</form></vxml>