<?xml version="1.0" encoding="UTF-8"?>

<vxml version="2.0" application="Hungama_RedFMRoot.vxml"
	xmlns="http://www.w3.org/2001/vxml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">

	<meta name="Hungama_RedFMUnsubMM.vxml" content="" />
	<meta name="Author" content="" />
	<meta name="Date" content="" />
	<meta name="Description" content="" />
	<meta name="Support" content="" />		<var name="NM_count" expr="1"/>	<var name="NI_count" expr="1"/>		<script>			aNavHistory.push("Hungama_RedFMUnsubMM.vxml");				var bill_min=new Object();	    	var billing_code=new Object();	    	UserStatus1=new Object();		    balance1=new Object();		    lang1=new Object();		    mode_sub1=new Object();		    PreviewToPlay1 = new Object();	 	</script>	<form id="Main">		<block>			<script srcexpr="'http://'+IP+'/'+servlet_path+'/Hungama_RedFMQUERY?ANI='+ANI+'&amp;TOKEN=1'"/>			<assign name="usr_status" expr="UserStatus1.value"/>			<log>Live Radio usr_status is	<value expr="usr_status"/>  </log>						<if cond="usr_status=='1'||usr_status == '11'">				<assign name="lang" expr="lang1.value"></assign>				<assign name="balance" expr="balance1.value"></assign>				<log>	lang is	<value expr="lang"/>  </log>				<log>	balance is	<value expr="balance"/>  </log>				 <goto next="#Main2"/>       		    	<elseif cond="usr_status == '-1'"/>	    		<goto next="#Nuser" />  					        				<elseif cond="usr_status == '0'"/>				<prompt bargein="true">				    <audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/subscription-pending-h.wav'" />				</prompt>				<goto next="End.vxml" />							<else/>		    	<prompt bargein="true">					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/insufficient-balance-h.wav'" />				</prompt>				<goto next="End.vxml" />			</if>			</block>	</form>	<form id="Nuser">		<field name="opt">			<grammar mode="dtmf" root="start"> 		    	<rule id="start" scope="public"> 		     		<one-of> 		     			 <item>  1	</item> 		       		</one-of> 		     	</rule>       		</grammar>			<prompt bargein="true">				    <audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/not_subscriber_mirchi-100-h.wav'" />			</prompt>			<filled>			<script srcexpr="'http://'+IP+'/'+servlet_path+'/WriteDTMFLogging?ANI='+ANI+'&amp;Prompt=welcome-new-live-radio&amp;DTMF='+opt+'&amp;DNIS='+DNIS+'&amp;event=LRADIO_SUB'"/>			<if cond="opt=='1'">					<goto next="Hungama_RedFMNewuser.vxml#DCONF"/>			</if>			</filled>						<nomatch cond="NM_count == 1">           		<assign name="NM_count" expr="NM_count+1"/>				<prompt>                  	<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalid-input-'+lang+'.wav'" />                 </prompt>					<reprompt></reprompt>		   	</nomatch>				   				   	<nomatch cond="NM_count == 2">		   		<assign name="NM_count" expr="NM_count+1"/>                <prompt>                     <audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalid-input-'+lang+'.wav'" />                </prompt>			 	<reprompt></reprompt>			</nomatch> 						<nomatch cond="NM_count == 3">					<goto next="End.vxml"/>			</nomatch> 						<noinput cond="NI_count == 1">				<assign name="NI_count" expr="NI_count+1"/>			    <prompt>                 	<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/no-response-'+lang+'.wav'" />                 </prompt>				<reprompt></reprompt>			</noinput>						<noinput cond="NI_count == 2">				<assign name="NI_count" expr="NI_count+1"/>			     <prompt>                             <audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/no-response-'+lang+'.wav'" />                 </prompt>				<reprompt></reprompt>			</noinput>						<noinput cond="NI_count == 3">				<goto next="End.vxml"/>			</noinput>						</field>	</form>	<form id="Main2">		<field name="option" >        	<grammar mode="dtmf" root="start"> 		    	<rule id="start" scope="public"> 		     		<one-of> 		     			 <item>  3	</item> 		       		</one-of> 		     	</rule>       		</grammar>       			        <prompt bargein="true" timeout="3s" >				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/unsubscriber_mirchi-'+lang+'.wav'"></audio>			</prompt>		       	        <filled>	        	        	<if cond="option=='3'">	        		<goto next="#Main_Menu"/>	        	<else/>	        		<clear namelist="option"/>	        		<throw event="nomatch"/>        	 	</if>        	</filled>          	       	<noinput count="1">	        	<prompt bargein="true">	       			<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/no-response-'+lang+'.wav'" />	       		</prompt>	       		<reprompt/>	        </noinput>        	<noinput count="2">	        	<prompt bargein="true">	       			<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/no-response-'+lang+'.wav'" />	       		</prompt>	       		<reprompt/>	        </noinput>        	<noinput count="3">	       		<goto next="End.vxml"/>	        </noinput>	        	        <nomatch count="1">	       		<prompt bargein="true">	       			<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalid-input-'+lang+'.wav'" />	       		</prompt>	       		<reprompt/>	       	</nomatch>	       	<nomatch count="2">	       		<prompt bargein="true">	       			<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalid-input-'+lang+'.wav'" />	       		</prompt>	       		<reprompt/>	        </nomatch>	       	<nomatch count="3">       			<goto next="End.vxml"/>	            	       	</nomatch>       </field>	</form>
	<form id="Main_Menu">		<field name="option" >        	<grammar mode="dtmf" root="start"> 		    	<rule id="start" scope="public"> 		     		<one-of> 		     			 <item>  1	</item> 		     		</one-of> 		     	</rule>       		</grammar>       			        <prompt bargein="true" timeout="3s" >				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/confirm-unsub-'+lang+'.wav'"></audio>			</prompt>		       	        <filled>	        	<if cond="option=='1'">	        		<script srcexpr="'http://'+IP+'/'+servlet_path+'/Hungama_RedFMUNSUB?ANI='+ANI+'&amp;unsub_rsn=USER_REQS'"/>	        		<prompt bargein="true" timeout="3s">	        		<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/unsubscribed-'+lang+'.wav'"></audio>	       			</prompt>	       		   <goto next="End.vxml"/>	        	<else/>	        		<clear namelist="option"/>	        		<throw event="nomatch"/>        	 	</if>        	</filled>          	       	<noinput count="1">	        	<prompt bargein="true">	       			<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/no-response-'+lang+'.wav'" />	       		</prompt>	       		<reprompt/>	        </noinput>        	<noinput count="2">	        	<prompt bargein="true">	       			<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/no-response-'+lang+'.wav'" />	       		</prompt>	       		<reprompt/>	        </noinput>        	<noinput count="3">	       		<goto next="End.vxml"/>	        </noinput>	        	        <nomatch count="1">	       		<prompt bargein="true">	       			<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalid-input-'+lang+'.wav'" />	       		</prompt>	       		<reprompt/>	       	</nomatch>	       	<nomatch count="2">	       		<prompt bargein="true">	       			<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalid-input-'+lang+'.wav'" />	       		</prompt>	       		<reprompt/>	        </nomatch>	       	<nomatch count="3">       			<goto next="End.vxml"/>	            	       	</nomatch>       </field>	</form></vxml>