	<var name="currenttime" expr="0"/>
	<script>
		function GetCurrentDate()
		{
			var currentDT = new Date();
			var day = currentDT.getDate();
			var month = currentDT.getMonth() + 1;
			var year = currentDT.getFullYear();
			if (month.length == 1) {month = "0" + month; }
			if (day.length == 1) {day = "0" + day; }
			var strDateTime =  day+""+month;
			return strDateTime;
		}
	</script>
     
     <script>
		var favSongs=new Object();
		var favLangs=new Object();
		var out_string=new Object();
		var len=0;
	</script>
<form id="MAINMENU">
	
	<block>
	<assign name="currenttime" expr="GetCurrentDate()"/>
	<log>current minute: <value expr="currenttime"/></log>

			<prompt bargein="true" timeout="2s" cond="circle!='hay'">
	
				<audio expr="promptspath_54646+'/'+lang+'/Welcome_catname'+sp_zone+'.wav'"/>
						
						 
				</prompt>
	 <goto next="#loadcliptoplay"/>
	</block>
	
			
		
		
</form>
	
		 	
	 <form id="loadcliptoplay">
		  <block> 
		  <if cond="circle=='hay'">
		  	<script srcexpr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=54646config_V2/'+operator+'/mwconfig/songconfig/navratrispecial/navratri_'+currenttime+'.cfg'"/>
		  <else/>
		  	<script srcexpr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=54646config_V2/'+operator+'/mwconfig/songconfig/specialzone/'+lang+playconfig+'_'+sp_zone+'.cfg'"/>
		   </if>
			<script>
						array.length=0;
						unike_play=0;
			</script>

					<assign name="len" expr="favSongs.length" />
					<assign name="movie_idx" expr="0" />
					<assign name="i" expr="0" />
			<script>
				<![CDATA[
					   for(k=0; k<len ;  k++)
					   {  	  
								array[k] = favSongs[k];						   
					   }

				]]>		
			</script> 	       					       
			<goto next="#PlayFullSongs"/>  					
		   </block>
	</form>

	
<form id="PlayFullSongs"> 
  
	 <field name="keypadoption">               
		    	 
		<grammar mode="dtmf" root="start" version="1.0">
