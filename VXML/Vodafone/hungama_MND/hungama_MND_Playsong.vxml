<?xml version="1.0" encoding="UTF-8"?><vxml version="2.0" application="hungama_MNDRoot.vxml" xmlns="http://www.w3.org/2001/vxml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">	<meta name="hungama_MND_Playsong.vxml" content=""/>	<meta name="Author" content=""/>	<meta name="Date" content=""/>	<meta name="Description" content=""/>	<meta name="Support" content=""/>			<form id="loadcliptoplay">	<block> 				<log expr="'playconfig isss'+playconfig"/>				<script srcexpr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=MNDconfig/'+lang+'/'+playconfig+'.cfg&TOKEN=FREE'"/>				<script>					array.length=0;					unike_play=0;				</script>				<assign name="songlen" expr="favSongs.length" />				<assign name="movie_idx" expr="'0'" />				<assign name="i" expr="'0'" />   				<script>						<![CDATA[						   for(k=0; k<songlen ;  k++)							  {  	  								array[k] = favSongs[k];						   							  }						]]>						</script> 												      				<log>	unike_play  isssss    <value expr="unike_play"/> 	</log>      				<log>	songlen  isssss    <value expr="songlen"/> 	</log>        				    				      				<prompt bargein="true" timeout="0s">							<audio expr="promptpath+'/'+lang+'/'+playconfig+'/mainmenu.wav'"/>					</prompt>					       					       			<goto next="#StartToPlaysongs"/>  						   </block>	</form>		<form id="StartToPlaysongs">		<block>				<if cond="unike_play &gt; songlen-1">						<assign name="unike_play" expr="0" />		        <elseif cond="unike_play &lt; 0" />						<assign name="unike_play" expr="songlen-1" />				</if>				<log expr="'UNIKE PLAY ISSSS--'+unike_play"/>				<log expr="'ARRAY[0] ISSSS--'+array[unike_play]"/>										<script>					var songcheck=array[unike_play].split("_");					contentdir=songcheck[0]; 					songID = array[unike_play];									</script>						 <assign name="song_begin" expr="starttime()"/>			 <assign name="song_listen" expr="'START'" />								 		</block>		<field name="selectoption">				<grammar mode="dtmf" root="start" version="1.0">			   		<rule id="start" scope="public">			   		  <one-of>					      <item>1</item>					      <item>2</item>						      <item>3</item>					      <item>6</item>					      <item>7</item>					      <item>8</item>					  </one-of>			   		</rule>		       </grammar>					      <prompt bargein="true"  timeout="1s">			      <audio expr="contentpath+contentdir+'/'+songID+'.wav'"/>		      </prompt> 			 		  		      <filled>		      			<assign name="contentchoice" expr="selectoption"/>		      			<goto next="#contentLogin"/>      		      </filled>				  				     			<noinput>			   	    	<assign name="contentchoice" expr="'3'"/>			   	    	<goto next="#contentLogin"/> 				</noinput>			<nomatch>					<assign name="contentchoice" expr="'2'"/>					<prompt bargein="true" timeout="0s">						<audio expr="promptpath+'/'+lang+'/'+playconfig+'/invalidoption.wav'"/>					</prompt>					<goto next="#contentLogin"/> 			</nomatch>		  </field>	  	  </form>		    <form id="contentLogin">  		<block>  				 		 <assign name="song_listen" expr="'END'" />						 <assign name="compare_time" expr="rim(song_begin)"/>						 <assign name="ContentTime" expr="AddContentTime(compare_time,ContentTime)"/>							 						 <assign name="contentlog" expr="contentlog+hash+songID+hash+cat+hash+subcat+hash+compare_time"/>						 <if cond="contentchoice=='1'">						 		<script> unike_play=unike_play-1;</script> 						 		<goto next="#StartToPlaysongs"/> 						 <elseif cond="contentchoice==2"/>						 		<script> unike_play=unike_play;</script>						 		<goto next="#StartToPlaysongs"/> 						 <elseif cond="contentchoice==3"/>						 		<script> unike_play=unike_play+1;</script>						 		<goto next="#StartToPlaysongs"/> 						 <elseif cond="contentchoice==6"/>						 		<goto next="#Keypadoption"/> 						 <elseif cond="contentchoice==8 || contentchoice==7"/>						 		<goto next="hungama_MND_SelectLang.vxml#CharacterMenu"/> 						 						 </if>  		</block>  </form>	<form id="Keypadoption"> 	<field name="choice">		<grammar mode="dtmf" root="start" version="1.0">			<rule id="start" scope="public">					<one-of>					 <item>  1  </item>					 <item>  2  </item>					 <item>  3  </item>					 <item>  4  </item>					 <item>  5  </item>					 <item>  8  </item>				</one-of>			</rule>		</grammar>				<prompt bargein="true" timeout="1s">						<audio expr="promptpath+'/'+lang+'/'+playconfig+'/keypad.wav'"/>				</prompt>		 <filled>						<if cond="choice=='1'">						 		<script> unike_play=unike_play-1;</script> 						 		<goto next="#StartToPlaysongs"/> 						 <elseif cond="choice==2"/>						 		<script> unike_play=unike_play;</script>						 		<goto next="#StartToPlaysongs"/> 						 <elseif cond="choice==3"/>						 		<script> unike_play=unike_play+1;</script>						 		<goto next="#StartToPlaysongs"/> 						 <elseif cond="choice==6"/>						 		<goto next="#Keypadoption"/> 						 <elseif cond="choice==8 || choice==7"/>						 		<goto next="hungama_MND_SelectLang.vxml#CharacterMenu"/> 						 						 </if>		     </filled>				    						 <noinput>				<assign name="index" expr="'0'" />					<script> unike_play=unike_play;</script>					<goto next="#StartToPlaysongs"/>			 </noinput>  			 			<nomatch>				<assign name="index" expr="'0'" />				<script> unike_play=unike_play;</script>					<goto next="#StartToPlaysongs"/> 		    </nomatch> 			    </field></form>	</vxml>