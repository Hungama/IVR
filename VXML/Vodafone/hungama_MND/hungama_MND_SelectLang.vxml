<?xml version="1.0" encoding="UTF-8"?><vxml version="2.0" application="hungama_MNDRoot.vxml" xmlns="http://www.w3.org/2001/vxml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">	<meta name="hungama_MND_SelectLang.vxml" content=""/>	<meta name="Author" content=""/>	<meta name="Date" content=""/>	<meta name="Description" content=""/>	<meta name="Support" content=""/>		<script>var len=0;			</script>	<form id="Welcome">	<block>		<assign name="C_Start" expr="start_time()"/> 		    <script>  				  realDNIS=session.connection.local.uri;                   				  ANI=session.connection.remote.uri;	   		    </script>	    	    	 			    	 	  <assign name="ANI" expr="msisdn(ANI)"/>				  <assign name="DNIS" expr="realDNIS"/>	    		                                         	  <log expr="'ANI @@@@@@@@@@@@@@@@@@@  =' + ANI"/>              	  <log expr="'DNIS @@@@@@@@@@@@@@@@@@@@ =' + DNIS"/>                      <script srcexpr="'http://'+IP+'/'+servlet_path+'/hungama_check?ANI='+ANI"/>	      			<assign name="ciropr" expr="ciropr1.value"/>	      			<log expr="'ciropr @@@@@@@@@@@@@@@@@@@@ =' + ciropr"/>	      			<script>							var splitstr=ciropr.split("#");							operator=splitstr[1];							circle=splitstr[2];					</script>	      				       					       	      		<log>circle  is	<value expr="circle"/>    </log>				<log>operator is <value expr="operator"/>    </log>								<assign name="operator" expr="'vodm'"/>								<assign name="contentlog" expr="ANI+hash+DNIS+hash+getcurryyyymmdd()+hash+C_Start"></assign>								<!-- Pre call  prompt playing -->								<if cond="circle=='mah' || circle=='tnu' || circle=='mum' || circle=='pun' || circle=='kol' || circle=='wbl'  || circle=='ori' || circle=='chn' || circle=='apd' || circle=='asm' || circle=='guj' || circle=='kar' || circle=='ker'">						<prompt bargein="true" timeout="0s">								<audio expr="promptpath+'/PrecallPrompts/tollmodal_3rs_'+circle+'.wav'"/>						</prompt>				<elseif cond="circle=='upe' || circle=='upw' || circle=='mpd' || circle=='hpd' || circle=='bih' || circle=='raj' || circle=='del' || circle=='har'"/>							<prompt bargein="true" timeout="0s">								<audio expr="promptpath+'/PrecallPrompts/tollmodal_3rs_hin.wav'"/>						</prompt>				<else/>						<prompt bargein="true" timeout="0s">								<audio expr="promptpath+'/PrecallPrompts/tollmodal_3rs_eng.wav'"/>						</prompt>				</if> 									<goto next="#CharacterMenu"/>		</block></form><form id="CharacterMenu">  <script>  	i=0;  	charcter.length=0;   </script>  <block>  			<assign name="index" expr="'0'" />		     <script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=MND_QUERY&amp;INTOKEN=2&amp;OUTTOKEN=1&amp;INPARAM[0]=1&amp;INPARAM[1]=1'"/>		         <script>				 <![CDATA[					   for(j=0; j< out_string.length;  j++)						  {  							      charcter[j] = out_string[j];						  }					]]>			</script>			<script>				 len=charcter.length;			</script>			<log expr="'len iss ' +len"></log>					   </block>   	<field name="choice">		<grammar mode="dtmf" root="start" version="1.0">			<rule id="start" scope="public">					<one-of>					 <item>  1  </item>					 <item>  2  </item>					 <item>  3  </item>					 <item>  4  </item>					 <item>  5  </item>					 <item>  8  </item>				</one-of>			</rule>		</grammar>				<prompt bargein="true" timeout="1s">					<foreach item="names" array="charcter">					     <!--<mark nameexpr="index++"/>-->						<log>	index isss    	<value expr="index"/>  	</log>						<log>	names is is    	<value expr="names"/>  	</log>						<audio expr="promptpath+'/'+lang+'/'+names+'/sub_'+names+'.wav'"/>						<audio expr="promptpath+'/'+lang+'/'+names+'/press_dtmf'+ ++index+'.wav'"/>					</foreach>																	</prompt>		 <filled>				<if cond="choice &lt;len+1">						<assign name="playconfig" expr="charcter[--choice]" />						<assign name="cat" expr="charcter[choice]" />						<assign name="subcat" expr="charcter[choice]" />						<goto next="hungama_MND_Playsong.vxml#loadcliptoplay"/>				<elseif cond="choice=='8'"/>					<goto nextitem="choice"/>			    <else/>						<assign name="index" expr="'0'" />				 		<audio expr="promptpath+'/'+lang+'/names/invalidoption.wav'"/>				        <goto nextitem="choice"/>			    </if>			     </filled>				    						 <noinput>				<assign name="index" expr="'0'" />				<audio expr="promptpath+'/'+lang+'/'+names+'/nodtmf.wav'"/>				<reprompt/> 			 </noinput>  			 			 <noinput count="3">					<assign name="playconfig" expr="charcter[0]" />					<assign name="cat" expr="playconfig" />					<assign name="subcat" expr="playconfig" />						<goto next="hungama_MND_Playsong.vxml#loadcliptoplay"/> 			 </noinput> 			              			<nomatch>				<assign name="index" expr="'0'" />				<audio expr="promptpath+'/'+lang+'/'+names+'/invalidoption.wav'"/>				<reprompt/>		    </nomatch> 			<nomatch count="3">					<assign name="playconfig" expr="charcter[0]" />					<assign name="cat" expr="playconfig" />					<assign name="subcat" expr="playconfig" />								<goto next="hungama_MND_Playsong.vxml#loadcliptoplay"/>           </nomatch>     </field></form>	</vxml>