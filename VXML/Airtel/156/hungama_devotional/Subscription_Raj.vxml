			</prompt>
			<goto next="#doubleconf"></goto>
		</if>		
		<if cond="double_switch=='1' &amp;&amp; prompt_switch=='0' &amp;&amp; price_point!='ten' &amp;&amp; circle!='kar'">
			<prompt bargein="true" timeout="1s">
				   <audio expr="promptpath+'switch/'+LANG_DESCRIPTION+'/subintro_rs2.wav'"/>
			</prompt>
			<goto next="#doubleconf"></goto>
		</if>
