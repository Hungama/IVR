			<prompt bargein="true" timeout="1s">
				   <audio expr="promptpath+'switch/'+LANG_DESCRIPTION+'/subintro_rs10.wav'"/>
			</prompt>
			<goto next="#doubleconf"></goto>
		</if>		
		<if cond="DirectSub== '1'">
			<if cond="price_point=='ten'">
				<assign name="amt" expr="'10'"/>
