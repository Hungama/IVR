			<script>
			 <![CDATA[
				 for(j=0; j< out_string.length;  j++)
					  {  	  
					       if(j==0)
						dayspl = out_string[j];			
					  }
				]]>
			</script>
			<log expr="'Day special is @@@@@@@@@ '+dayspl"></log>
			
			<prompt bargein="false" timeout="0s">
				<audio expr="promptpath+LANG_DESCRIPTION+'/welcome_'+dayspl+'.wav'"/>
			</prompt>
		</if>
