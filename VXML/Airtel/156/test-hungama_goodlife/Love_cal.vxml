				   </script>
		
				   <log expr="'@@@@@@ used balance is '+total_used_bal"/>
				   <log expr="'@@@@@@ left balance is '+curbalance"/>
		 		   <if cond="curbalance &lt;= '0'">          
 				      <goto next="Topup.vxml"/>
				   </if>
				</if>

					<goto next="AccountManager_LIFE.vxml"/>
			  <else/>
					<goto next="#lovecalc"/>
			  </if> 	
			</filled>
		</field>
		
		<noinput>
			<goto next="#lovecalc"/>
		</noinput>
		
		
		<nomatch>
			<prompt bargein="true" timeout="0s">
				<audio expr="promptpath_lifestyle+'/'+lang+'/invalidoption.wav'"/>
			</prompt>	
			<goto next="#START_SELF"/>

		</nomatch>
	</form>



<form id="lovecalc">
		<field name="MOB" type="digits?length=8">	
	
			<!--<grammar mode="dtmf" root="start1" version="1.0">
				<rule id="start1" scope="public">	
					<one-of>
						 <item>  1  </item>
						 <item>  2  </item>
						 <item>  3  </item>
						 <item>  4  </item>
						 <item>  5  </item>
						 <item>  6  </item>
						 <item>  7  </item>
						 <item>  8  </item>
						 <item>  9  </item>
						 <item>  0  </item>
					</one-of>
				</rule>
			</grammar>-->

    		        <prompt bargein="true" timeout="17s">
				<audio expr="promptpath_lifestyle+'/'+lang+'/beep.wav'"/>				
			</prompt>
			<filled>
				<log>pressed dtmf:<value expr="MOB"/></log>
				
				<script>
						MOB=MOB.replace(/\s+/g,'');
						number = MOB.length;
						firstdob=MOB;
				</script>
				
				<if cond="MOB==''">
					<prompt bargein="true" timeout="0s">
						<audio expr="promptpath_lifestyle+'/'+lang+'/nodtmf.wav'"/>
					</prompt>	
					<goto next="#START_SELF"/>
				</if>
				<if cond="number!='8'">
					<prompt bargein="true" timeout="0s">
						<audio expr="promptpath_lifestyle+'/'+lang+'/invalidoption.wav'"/>
					</prompt>	
					<goto next="#START_SELF"/>
				</if>
				<goto next="#submit_DOB"></goto>
			</filled>
			<noinput count="1">
					<prompt bargein="true" timeout="0s">
					<audio expr="promptpath_lifestyle+'/'+lang+'/nodtmf.wav'"/>
					</prompt>			
					<goto next="#START_SELF"/>
			</noinput>
			<noinput count="2">
					<prompt bargein="true" timeout="0s">
					<audio expr="promptpath_lifestyle+'/'+lang+'/nodtmf.wav'"/>
					</prompt>			
					<goto next="#START_SELF"/>
			</noinput>
			<noinput count="3">
					<goto next="Hungama_Lifestyle_Mainmenu.vxml"/>
			</noinput>
			<nomatch count="1">
					<prompt bargein="true" timeout="0s">
					<audio expr="promptpath_lifestyle+'/'+lang+'/invalidoption.wav'"/>
					</prompt>			
					<goto next="#START_SELF"/>
			</nomatch>
			<nomatch count="2">
					<prompt bargein="true" timeout="0s">
					<audio expr="promptpath_lifestyle+'/'+lang+'/invalidoption.wav'"/>
					</prompt>			
					<goto next="#START_SELF"/>
			</nomatch>
			<nomatch count="3">
					<goto next="Hungama_Lifestyle_Mainmenu.vxml"/>
			</nomatch>
			
		</field>
</form>
<form id="submit_DOB">
	<field name="choice">
	       <grammar mode="dtmf" root="start" version="1.0">
			<rule id="start" scope="public">
				<one-of>
					 <item>  *  </item>
				</one-of>
			</rule>
		</grammar>
			<filled>
			    <if cond="choice=='*'">
				<goto next="#check"/>	
			    </if> 	
			</filled>
		</field>
		
			<noinput count="1">
					<prompt bargein="true" timeout="0s">
					<audio expr="promptpath_lifestyle+'/'+lang+'/nodtmf.wav'"/>
					</prompt>			
					<goto next="#START_SELF"/>
			</noinput>
			<noinput count="2">
