	<meta name="Love_cal.vxml" content="" />
	<meta name="Author" content="" />
	<meta name="Date" content="" />
	<meta name="Description" content="" />
	<meta name="Support" content="" />	
	<var name="number" expr="'0'"/>
	<var name ="MOB" expr =""/>

<form id="START">	
	<block>
				<assign name="Section_Name" expr="'LOVE_CALCULATOR'"/>
				<if cond="timerflag=='1'">                       						   
				   <script srcexpr="'http://'+IP+'/hungama/duration?starttime='+initial_balance+'&amp;endtime='+call_time()"/>
				   <script>
				     total_used_bal=dur;
				     curbalance=balance-total_used_bal;  						  
				   </script>
		
				   <log expr="'@@@@@@ used balance is '+total_used_bal"/>
				   <log expr="'@@@@@@ left balance is '+curbalance"/>
		 		   <if cond="curbalance &lt;= '0'">          
 				      <goto next="Topup.vxml"/>
				   </if>
				</if>

		<log expr="'############ In START Menu #############'"></log>
	</block>
	<field name="choice">
	       <grammar mode="dtmf" root="start" version="1.0">
			<rule id="start" scope="public">
				<one-of>
					 <item>  1  </item>
					 <item>  2  </item>
					 <item>  3  </item>
					 <item>  4  </item>
					 <item>  5  </item>
					 <item>  6  </item>
					 <item>  7  </item>
					 <item>  8  </item>
					 <item>  9  </item>
					 <item>  *  </item>
					 <item>  #  </item>
				</one-of>
			</rule>
		</grammar>

		<prompt bargein="true" timeout="0s">
			<audio expr="promptpath_lifestyle+'/'+lang+'/welcome_cat_lovecalc.wav'"/>
		</prompt>
			<filled>
			    <if cond="choice=='8'">
				<if cond="timerflag=='1'">                       						   
				   <script srcexpr="'http://'+IP+'/hungama/duration?starttime='+initial_balance+'&amp;endtime='+call_time()"/>
				   <script>
				     total_used_bal=dur;
				     curbalance=balance-total_used_bal;  						  
				   </script>
		
				   <log expr="'@@@@@@ used balance is '+total_used_bal"/>
				   <log expr="'@@@@@@ left balance is '+curbalance"/>
		 		   <if cond="curbalance &lt;= '0'">          
 				      <goto next="Topup.vxml"/>
				   </if>
				</if>
	
					<goto next="Hungama_Lifestyle_Mainmenu.vxml"/>
			  <elseif cond="choice=='9'"/>
				<if cond="timerflag=='1'">                       						   
				   <script srcexpr="'http://'+IP+'/hungama/duration?starttime='+initial_balance+'&amp;endtime='+call_time()"/>
				   <script>
				     total_used_bal=dur;
				     curbalance=balance-total_used_bal;  						  
				   </script>
		
				   <log expr="'@@@@@@ used balance is '+total_used_bal"/>
				   <log expr="'@@@@@@ left balance is '+curbalance"/>
		 		   <if cond="curbalance &lt;= '0'">          
 				      <goto next="Topup.vxml"/>
				   </if>
				</if>

					<goto next="AccountManager_LIFE.vxml"/>
			  <else/>
					<goto next="#START_SELF"/>
			  </if> 	
			</filled>
		</field>
		
		<noinput>
			<goto next="#START_SELF"/>
		</noinput>
		
		
		<nomatch>
			<goto next="#START_SELF"/>
		</nomatch>
	</form>



<form id="START_SELF">	
