				   </script>
		
				   <log expr="'@@@@@@ used balance is '+total_used_bal"/>
				   <log expr="'@@@@@@ left balance is '+curbalance"/>
		 		   <if cond="curbalance &lt;= '0'">          
 				      <goto next="Topup.vxml"/>
				   </if>
				</if>
	
					<goto next="Hungama_Lifestyle_Mainmenu.vxml"/>
			  <elseif cond="choice=='9'"/>
				<if cond="timerflag=='1'">                       						   
