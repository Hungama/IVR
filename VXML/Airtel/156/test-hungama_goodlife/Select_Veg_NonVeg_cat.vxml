<?xml version="1.0" encoding="UTF-8"?><vxml version="2.0" application="rasio_root.vxml"	xmlns="http://www.w3.org/2001/vxml"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">	<meta name="Select_Veg_NonVeg_cat.vxml" content="" />	<meta name="Author" content="" />	<meta name="Date" content="" />	<meta name="Description" content="" />	<meta name="Support" content="" />				  <form id="categorymenu">        		 	    <field name="choice">			 <grammar mode="dtmf" root="start">               <rule id="start" scope="public">                 <one-of>				 				  <item> 1 </item>				  <item> 2 </item>				                 </one-of>              </rule>            </grammar>					   			<prompt bargein="true" timeout="3s">				<audio expr="'http://'+CDSIP+CDSPATH+'promptFiles/sanjeev_rasoi/'+LANG_DESCRIPTION+'/'+'Select_recipe_type.wav'"/>			</prompt>	      		            					<filled>				<if cond="choice == '1'">				      <goto next="Rasoi_MainMenu_Veg.vxml"/>						</if>				<if cond="choice == '2'">					 <goto next="Rasoi_MainMenu_NonVeg.vxml"/>				</if>						</filled>						 <noinput>			     <audio expr="'http://'+CDSIP+CDSPATH+'promptFiles/sanjeev_rasoi/'+LANG_DESCRIPTION+'/'+'waiting_response.wav'"/>                                 <reprompt/>              </noinput>                     	        		     <nomatch>				 <audio expr="'http://'+CDSIP+CDSPATH+'promptFiles/sanjeev_rasoi/'+LANG_DESCRIPTION+'/'+'invalidoption.wav'"/>						 <reprompt/>		     </nomatch> 	                             							  </field>	     </form> </vxml>