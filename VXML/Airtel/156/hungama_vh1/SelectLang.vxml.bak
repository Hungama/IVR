<form id="TnBSub">
<block>
<log>Enter in TnB Block</log>
</block>
	<field name="welcome" >
		    	
		    <grammar mode="dtmf" root="start">
                   <rule id="start" scope="public">
                     <one-of>
				   <item>1</item>
				  <item>2</item>
                    </one-of>
                  </rule>
               </grammar> 
		    
		    			

				<prompt bargein="true" timeout="0s" cond="circle=='del' &amp;&amp; TnBStatus== '0' &amp;&amp;DNIS!='5584112&amp;&amp; TnBMode== 'TIVR'">
								<audio expr="promptpath+LANG_DESCRIPTION+version+'/subintro_Rs2day_15mins.wav'"/>
									
				 </prompt>
				    <prompt bargein="true" timeout="0s" cond="circle!='del' &amp;&amp;DNIS!='5584112&amp;&amp; TnBStatus== '0'"  >
						    <audio expr="promptpath+LANG_DESCRIPTION+version+'/subintro_Rs2day_2paisesec.wav'"/>
				    </prompt>
				
		        		
						<catch event="noinput">
							<goto nextitem="welcome"/>
						</catch>
						<catch event="error.badfetch">
							<goto nextitem="welcome"/>
						</catch>
					
		<filled>
			<if cond="welcome == '2'">
							<log>Sub Url:<value expr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=JBOX_SUB_Tnb_LIVE&amp;INTOKEN=7&amp;OUTTOKEN=1&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+lang+'&amp;INPARAM[2]=IVR&amp;INPARAM[3]='+DNIS+'&amp;INPARAM[4]=2&amp;INPARAM[5]=1507&amp;INPARAM[6]=47'"/></log>
							

							<script srcexpr="'http://'+IP+'/'+SERVLETPATH+'/'+dbservlet+'?PROCEDURE=JBOX_SUB_Tnb_LIVE&amp;INTOKEN=7&amp;OUTTOKEN=1&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+lang+'&amp;INPARAM[2]=IVR&amp;INPARAM[3]='+DNIS+'&amp;INPARAM[4]=2&amp;INPARAM[5]=1507&amp;INPARAM[6]=47'"/>
				  			
				  			<script>
												 <![CDATA[
												    for(j=0; j< out_string.length;  j++)
													  {  	  
														if(j==0)
														usr_status = out_string[j];
													       if(j==1)
														lang = out_string[j];
													       if(j==2)
														balance = out_string[j];
														
														if(j==4)
														TnBStatus= out_string[j];
														if(j==5)
														TnBMode= out_string[j];		

											
														  }

													]]>
												</script>
		<log>length of outstring  is	<value expr="out_string.length"/></log>
		<log>@@@@@@@New usr_status	<value expr="usr_status"/></log>		
		<log>@@@@@@@ lang is	<value expr="lang"/></log>		
		<log>@@@@@@@ balance minutes is	<value expr="balance"/></log>		
	
		
		<log>@@@@@@@ TnBMode is	<value expr="TnBMode"/></log>

		<log>@@@@@@@ TnBStatus is	<value expr="TnBStatus"/></log>
			
			<goto next="#prompt_play"/>
                <elseif cond="welcome=='1'"/>
			<assign name="subamt" expr="'2'"/>
			<goto next="#double_conf_1"></goto>               	
		 <else/>
				<goto next="#prompt_play"/>
		</if>
					
	</filled>
					
	</field>

</form>


