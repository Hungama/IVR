<?xml version="1.0" encoding="UTF-8"?>

<vxml version="2.0" application="rasio_root.vxml"
	xmlns="http://www.w3.org/2001/vxml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">

	<meta name="multi_model.vxml" content="" />
	<meta name="Author" content="" />
	<meta name="Date" content="" />
	<meta name="Description" content="" />
	<meta name="Support" content="" />	<var name="play" expr="''"/><var name="part" expr="1"/><var name="logs" expr="''"/><var name="ing_repeat" expr="'TRUE'"/><var name="c" expr="1"/><var name="bad_fetch" expr="'FALSE'"/><script>		var favSongs=new Object();		var out_string=new Object();		var dishes=new Array();		var missing=0;			var recipe_start=1;			</script>
<form id="main"><block> <if cond="timerflag=='1'">                       						   						 <script srcexpr="'http://10.2.73.158:8080/hungama/duration?starttime='+initial_balance+'&amp;endtime='+call_time()"/>						 <script>					    	 total_used_bal=dur;						     curbalance=balance-total_used_bal;  						  					     </script>					   					   <log expr="'@@@@@@ used balance is '+total_used_bal"/>					   <log expr="'@@@@@@ left balance is '+curbalance"/>                         <if cond="curbalance &lt;= '0'">                                       <goto next="Topup.vxml"/>                        </if>    </if> <script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RASOI_METADATA&amp;INTOKEN=2&amp;OUTTOKEN=1&amp;INPARAM[0]='+dishname+'&amp;INPARAM[1]='+LANG_DESCRIPTION"/>		<script>		 <![CDATA[			    for(j=0; j< out_string.length;  j++)				  {  	  				       if(j==0)					UNIQUE_CODE = out_string[j];				       if(j==1)					INGRADIENT_PART = out_string[j];				       if(j==2)					RECIPE_PART = out_string[j];				  }			]]>		</script>						<log>$$$$UNIQUE_CODE  is	<value expr="UNIQUE_CODE"/>    </log>						<log>$$$$INGRADIENT_PART  is	<value expr="INGRADIENT_PART"/>    </log>						<log>$$$$RECIPE_PART  is	<value expr="RECIPE_PART"/>    </log>						<log>$$$$dishname  is	<value expr="dishname"/>    </log>				<if cond="UNIQUE_CODE=='-1'">		<goto next="Rasoi_MainMenu.vxml"/>		</if>					<if cond="key &lt; 4">		<script>				key=key+1;			</script>		</if></block>			<field name="choice">						<grammar mode="dtmf" root="start">						<rule id="start" scope="public">							<one-of>								 															 <item>  9  </item>														</one-of>						</rule>				</grammar>								<prompt bargein="true" timeout="1s" cond="LANG_DESCRIPTION=='HIN'">						<audio expr="promptpath+LANG_DESCRIPTION+'/chefintro.wav'"/>					</prompt>					<filled>					<if cond="choice=='8'">					<assign name="OTHER_RECIPES" expr="'TRUE'"/>					<goto next="Rasoi_MainMenu.vxml"/>				</if>				<if cond="choice=='9'">					<assign name="OTHER_RECIPES" expr="'TRUE'"/>					<goto next="AccountManager.vxml"/>				</if><!--				<if cond="choice=='7'">					<goto next="#navigation"/>					</if>	-->			<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RASOI_INGRADENT&amp;INTOKEN=2&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+dishname"/>										<goto next="#ingredient"/>									</filled>		<noinput>			<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RASOI_INGRADENT&amp;INTOKEN=2&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+dishname"/>			<goto next="#ingredient"/>		</noinput>		<nomatch>		 <audio expr="promptpath+LANG_DESCRIPTION+'/'+'invalidoption_m.wav'"/>				 <reprompt/>		</nomatch></field></form>
<form id="ingredient">		<block>								<!-- missing ingredient -->										<script srcexpr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=rasoiconfig/'+operator+'/songconfig/missing.cfg'"/>					<assign name="lenMissing" expr="favSongs.length" />										<log>lenMissing  is	<value expr="lenMissing"/>    </log>					<log>dishname  is	<value expr="dishname"/>    </log>					<log>missing  is	<value expr="missing"/>    </log>										<script>						 <![CDATA[							for(j=0; j<lenMissing;  j++)							{  	  																	if(favSongs[j] == dishname)								{									  missing=1;									}															}						]]>							</script> 	       				<log>missing  is	<value expr="missing"/>    </log>					<if cond="missing==1">						<goto next="#playcontent"/>					</if>		</block> 	    <field name="sms_sender">			 <grammar mode="dtmf" root="start" version="1.0">               <rule id="start" scope="public">                 <one-of>				 				  <item> 8 </item>				  <item> 7 </item>				  <item> 9 </item>				                 </one-of>              </rule>            </grammar>            	<prompt bargein="false" timeout="1s" cond="ing_repeat=='TRUE'">						<audio expr="promptpath+LANG_DESCRIPTION+'/ingredients.wav'"/>						<audio expr="contentpath+'/rasoi/hindi/'+dishname+'.wav'"/>						<audio expr="promptpath+'/HIN/Ki.wav'"/>						<audio expr="promptpath+LANG_DESCRIPTION+'/pen_paper.wav'"/>				</prompt>						   <prompt bargein="true" timeout="0s">				 <audio expr="contentpath+'/rasoi/'+LANG_DESCRIPTION+'/'+UNIQUE_CODE+'_Ing_'+c+'.wav'"/>			   </prompt>						<filled>			     <script>			    	 c=c+1;			     </script>			     <assign name="ing_repeat" expr="'FALSE'"/>			     <if cond="sms_sender=='9'">			     	<goto next="AccountManager.vxml"/>			     <elseif cond="sms_sender=='8'"/>			     	<goto next="Rasoi_MainMenu.vxml"/>			     </if>			     <if cond="c &lt; INGRADIENT_PART">  	     				<goto next="#ingredient"/>	     		 <else/>	     		 		<goto next="#concern"/>	     		 </if>	     					</filled>			 	                             							 	<noinput>	     <script>	     	c=c+1;	     </script>			     <assign name="ing_repeat" expr="'FALSE'"/>				     <if cond="c &lt; INGRADIENT_PART">  	     			<goto next="#ingredient"/>	     		 <else/>	     		 	<goto next="#concern"/>	     		 </if> 	</noinput>             	            <nomatch>		 <audio expr="promptpath+LANG_DESCRIPTION+'/'+'invalidoption_m.wav'"/>				 <reprompt/>    </nomatch>	</field>	<catch event="error.badfetch">		<goto next="#concern"/>	</catch>		</form><form id="concern"> 	    <field name="choice_con">			 <grammar mode="dtmf" root="start" version="1.0">               <rule id="start" scope="public">                 <one-of>				 				  <item> 1 </item>				  <item> 9 </item>				                 </one-of>              </rule>            </grammar>					<prompt bargein="true" timeout="3s">				<audio expr="promptpath+LANG_DESCRIPTION+'/making_recipe.wav'"/>			</prompt>									<filled>				<if cond="choice_con == '1'">				   <script>				   c=1;				   </script>				   <goto next="#ingredient"/>				<elseif cond="choice_con =='9'"/>					<goto next="AccountManager.vxml"/>				<else/>					<goto next="#playcontent"/>					</if>			</filled>			 	                             							  <noinput>	     <goto next="#playcontent"/>	     </noinput>             	             <nomatch>		 <audio expr="promptpath+LANG_DESCRIPTION+'/'+'invalidoption_m.wav'"/>				 <reprompt/>     </nomatch>	  </field></form><form id="playcontent">		<block>					    <if cond="timerflag=='1'">                       						   						 <script srcexpr="'http://10.2.73.158:8080/hungama/duration?starttime='+initial_balance+'&amp;endtime='+call_time()"/>						 <script>					    	 total_used_bal=dur;						     curbalance=balance-total_used_bal;  						  					     </script>					   					   <log expr="'@@@@@@ used balance is '+total_used_bal"/>					   <log expr="'@@@@@@ left balance is '+curbalance"/>                         <if cond="curbalance &lt;= '0'">                                       <goto next="Topup.vxml"/>                        </if>    		</if>		<if cond="recipe_start=='1'">														<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RASOI_METADATA&amp;INTOKEN=2&amp;OUTTOKEN=1&amp;INPARAM[0]='+dishname+'&amp;INPARAM[1]='+LANG_DESCRIPTION"/>		<script>		 <![CDATA[			    for(j=0; j< out_string.length;  j++)				  {  	  				       if(j==0)					UNIQUE_CODE = out_string[j];				       if(j==1)					INGRADIENT_PART = out_string[j];				       if(j==2)					RECIPE_PART = out_string[j];				  }			]]>		</script>			<if cond="UNIQUE_CODE=='-1'">				<assign name="part" expr="1"/>  				<script> logs=1</script>				<goto next="#contentlogs"/>		</if>			 <assign name="song_begin" expr="start_time()"/>		 	 <assign name="song_listen" expr="'START'" />		 	 <assign name="songID" expr="UNIQUE_CODE"/>		</if>		 		</block>		<field name="choice">						<grammar mode="dtmf" root="start">						<rule id="start" scope="public">							<one-of>								 <item>  0  </item>															 <item>  8  </item>								 <item>  9  </item>							</one-of>						</rule>				</grammar>						<prompt bargein="true" timeout="1s">					<audio expr="contentpath+'/rasoi/'+LANG_DESCRIPTION+'/'+UNIQUE_CODE+'_'+part+'.wav'"/> 										</prompt>					<filled>				<assign name="logs" expr="choice"/>						<goto next="#contentlogs"/>		</filled>		<noinput>			<if cond="part &lt; RECIPE_PART">								<script> 				part=part+1;				recipe_start=0;				</script>				<goto next="#playcontent" />			<else/>				<assign name="part" expr="1"/>  				<script> logs=11</script>				<goto next="#contentlogs" />			</if>						</noinput>		<nomatch>		 <audio expr="promptpath+LANG_DESCRIPTION+'/'+'invalidoption_m.wav'"/>				 <reprompt/>		</nomatch>		</field>			</form>		<form id="contentlogs"><block>								 <assign name="song_listen" expr="'END'" />								 <log> @@@@@@ content start time  <value expr="song_begin"/> </log>								 								 <assign name="compare_time" expr="time_duration(song_begin)"/>								 <log>@@@@@@@ content listen time  <value expr="compare_time"/> </log>								 <assign name="ContentTime" expr="AddContentTime(compare_time,ContentTime)"/>								 <log>@@@@@@@ content add time  <value expr="ContentTime"/> </log>							 								 <assign name="contentlog" expr="contentlog+hash+songID+hash+cat+hash+subcat+hash+compare_time"></assign>  <if cond="timerflag=='1'">                       						   						 <script srcexpr="'http://10.2.73.158:8080/hungama/duration?starttime='+initial_balance+'&amp;endtime='+call_time()"/>						 <script>					    	 total_used_bal=dur;						     curbalance=balance-total_used_bal;  						  					     </script>					   					   <log expr="'@@@@@@ used balance is '+total_used_bal"/>					   <log expr="'@@@@@@ left balance is '+curbalance"/>                         <if cond="curbalance &lt;= '0'">                                       <goto next="Topup.vxml"/>                        </if>    </if> 								 				<if cond="logs=='5'"> 							<goto next="#pause"/>				<elseif cond="logs=='0'"/>												<goto next="#AddFavorite"/>												<elseif cond="logs=='8'"/>								<goto next="Rasoi_MainMenu.vxml"/>								<elseif cond="logs=='9'"/>								<goto next="AccountManager.vxml"/>												<elseif cond="logs=='11'"/>																			<goto next="#nav-noinput"/>								<else/>									<prompt bargein="true" timeout="1s">									<audio expr="promptpath+LANG_DESCRIPTION+'/invalidoption_m.wav'"/>								</prompt>								<goto next="#playcontent" />                </if></block></form>	<form id="AddFavorite">		<block>			<if cond="play=='content'">		    				<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RASOI_FAVORITE&amp;INTOKEN=3&amp;OUTTOKEN=1&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]=ADD&amp;INPARAM[2]='+LANG_DESCRIPTION+'/'+songID"/>			<else/>				<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RASOI_FAVORITE&amp;INTOKEN=3&amp;OUTTOKEN=1&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]=ADD&amp;INPARAM[2]='+LANG_DESCRIPTION+'/'+songID"/>			</if>			<script>			<![CDATA[			    for(j=0; j< out_string.length;  j++)				  {  	  				       if(j==0)							fav_status = out_string[j];				       				  }			]]>			</script>			<prompt bargein="true" timeout="0s">				<audio expr="promptpath+LANG_DESCRIPTION+'/recipe_add.wav'"/>			</prompt>			<goto next="#playcontent"/>					</block>	</form>		<form id="pause">			<field name="pause_opt">	        	<grammar mode="dtmf" root="start">					<rule id="start" scope="public">						<one-of>							 <item>  5  </item>							 												</one-of>					</rule>				</grammar>	    		<prompt bargein="true" timeout="5s">				<audio expr="promptpath+LANG_DESCRIPTION+'/resume_service.wav'"/>							</prompt>			<filled>				<goto next="#playcontent" />				</filled>	        </field>	    	<noinput>	    		<goto next="#pause" />	    	</noinput>	    	<nomatch>		        <goto next="#pause" />	    	</nomatch>	</form>			<form id="nav-noinput">			<field name="choice">			 <grammar mode="dtmf" root="start" version="1.0">               <rule id="start" scope="public">                 <one-of>				                 	  <item> 1 </item> 					  <item> 2 </item>                </one-of>              </rule>            </grammar>			<prompt bargein="true" timeout="3s" cond="missing=='0'">				<audio expr="promptpath+LANG_DESCRIPTION+'/recipe_complete.wav'"/>			</prompt>						<prompt bargein="true" timeout="3s" cond="missing=='1'">				<audio expr="promptpath+LANG_DESCRIPTION+'/recipe_complete1.wav'"/>			</prompt>						<filled>				<if cond="choice == '2'">				    <assign name="part" expr="1"/>                    <goto next="#playcontent" />                <elseif cond="choice=='1'"/>                	<script>                	c=1;                	</script>                	<goto next="#ingredient"/>				<else/>					<goto next="Rasoi_MainMenu.vxml"/>				</if>			</filled>			 	                             							  </field>	  <noinput>	     <goto next="Rasoi_MainMenu.vxml"/>     </noinput>             	             <nomatch>		 <goto next="Rasoi_MainMenu.vxml"/>     </nomatch> 	 </form>
</vxml>