			<script srcexpr="'http://'+IP+'/'+servlet_path+'/Consent_log?ANI='+ANI+'&amp;circle='+circle+'&amp;starttime='+C_Start+'&amp;service=1513&amp;dnis='+DNIS+'&amp;realdnis='+realDNIS+'&amp;consent=secondconsent&amp;response_code='+confirmUserChoice.userInput+'&amp;sessonid='+sessionId"/>

			<if cond="confirmUserChoice.userInput=='maxError'">
				
						<!--<script srcexpr="'http://'+IP+'/'+servlet_path+'/SwitchCall_log?ANI='+ANI+'&amp;circle='+circle+'&amp;starttime='+C_Start+'&amp;service=MND&amp;dnis='+DNIS+'&amp;realdnis='+realDNIS+hash+'maxError'"/> -->
					
				<goto next="MND_end.vxml"></goto>
			<elseif cond = "confirmUserChoice.userInput=='submitPackChosen'"/>
				<goto next="#BillStatusChk"/>
			<elseif cond = "confirmUserChoice.userInput=='goBackToPackChoices'"/>
				
					<!--	<script srcexpr="'http://'+IP+'/'+servlet_path+'/SwitchCall_log?ANI='+ANI+'&amp;circle='+circle+'&amp;starttime='+C_Start+'&amp;service=MND&amp;dnis='+DNIS+'&amp;realdnis='+realDNIS+hash+'Star'"/> -->
					
				<goto next="#SubMainmenu"/>
			<elseif cond = "confirmUserChoice.userInput=='callDrop'"/>
				
					<!--	<script srcexpr="'http://'+IP+'/'+servlet_path+'/SwitchCall_log?ANI='+ANI+'&amp;circle='+circle+'&amp;starttime='+C_Start+'&amp;service=MND&amp;dnis='+DNIS+'&amp;realdnis='+realDNIS+hash+'callDrop'"/> -->
					
					<goto next="MND_end.vxml"></goto>

			<else/>
				<goto next="#SubMainmenu"/>
			</if>			
		</filled>
	</subdialog>

</form>



<form id="BillStatusChk">
	<block>
		<assign name="prompt_name" expr="'BILLING_CHECK'"/>
		<assign name="dtmfsub" expr="'Second_level_input'"/>
		<assign name="dtmf_input" expr="dtmf_input+'@'+choice"></assign>
		<assign name="dtmf_log" expr="dtmf_log+hash+dtmfsub+hash+choice"></assign>
		<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=MND_SUB&amp;INTOKEN=7&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+lang+'&amp;INPARAM[2]=IVR&amp;INPARAM[3]='+DNIS+'&amp;INPARAM[4]='+amt+'&amp;INPARAM[5]='+service_id+'&amp;INPARAM[6]='+plan_id"/>         
		
		
		<!--	<script srcexpr="'http://'+IP+'/'+servlet_path+'/SwitchCall_log?ANI='+ANI+'&amp;circle='+circle+'&amp;starttime='+C_Start+'&amp;service=MND&amp;dnis='+DNIS+'&amp;realdnis='+realDNIS+hash+'Five'"/> -->
		<if cond="double_switch=='1'">
