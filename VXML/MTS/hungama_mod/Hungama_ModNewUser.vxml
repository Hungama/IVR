					<assign name="var_wel" expr="getRandom(1,5)"/>
					<prompt bargein="true" timeout="0s" cond="usr_status=='-1'&amp;&amp; directaccess!='SAU' ">
							<audio expr="Prompt_Path+'/01/lang_option_'+circle+'_'+var_wel+'.wav'"/>
												
					</prompt>
						<prompt bargein="true" timeout="0s" cond="usr_status=='-1'&amp;&amp; directaccess=='SAU' ">
							<audio expr="Prompt_Path+'/'+lang+'/lang_option_'+circle+'.wav'"/>
						</prompt>
					<if cond="circle=='del' || circle=='guj' || circle=='hry' || circle=='mum' || circle=='raj' || circle=='upe' || circle=='upw' || circle=='apd' ||circle=='bih' || circle=='ker' || circle=='kar' ||circle=='kol'|| circle=='mah' || circle=='tnu' ||circle=='wbl'">
						<script srcexpr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=config/'+operator+'/'+circle+'/langorder_existing.cfg'"/>
