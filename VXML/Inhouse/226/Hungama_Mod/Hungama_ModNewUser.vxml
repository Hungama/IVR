	<form id="LANGSELDEL">
			<block>
				<log>Delhi block</log>
				<assign name="program_name" expr="'Hungama_ModNewUser_LANGSELDEL'"/>
				<prompt bargein="true" timeout="0s" cond="usr_status=='-1'">
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/common/lang_option_'+lang+'.wav'"/>
				</prompt>

			<if cond="usr_status=='-1'">
				<log>Get Lang:<value expr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=config/'+operator+'/'+circle+'/langorderNewUser'+circle+'.cfg'"/></log>

				<script srcexpr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=config/'+operator+'/'+circle+'/langorderNewUser'+circle+'.cfg'"/>

			<else/>
				<log>Get Lang:<value expr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=config/'+operator+'/'+circle+'/langorder.cfg'"/></log>

				<script srcexpr="'http://'+IP+'/'+servlet_path+'/ar_ReadConfigSong?ConfigPath=config/'+operator+'/'+circle+'/langorder.cfg'"/>
			</if>


				<script>
						    	array1.length=0;
				</script>
				<assign name="movie_idx" expr="0"/>
				<assign name="len" expr="4" />
				<assign name="maxlang" expr="4" />
				<assign name="i" expr="0" />
				<log>previous Array[1]  is	<value expr="array1[0]"/>    </log>

				<script>
					 <![CDATA[
						   for(j=0; j< 4 ;  j++)
						   {  	
							var x=	 favSongs[j];
							if(x!=null)
							{
							array1[i] = favSongs[j];	
							i++;
							}					   
						   }
	
					]]>		
				</script> 
 	       					       
			
				<log>i value is:<value expr="i"/></log>
  
				<log>Length  is	<value expr="len"/>    </log>
				<log>Array[1]  is	<value expr="array1[0]"/>    </log>
				<log>Array[1] 1 is	<value expr="array1[1]"/>    </log>	
			</block>
			<field name="choice">
					<grammar mode="dtmf" root="start1" version="1.0">
							<rule id="start1" scope="public">
								<one-of>
									 <item>  1  </item>
									 <item>  2  </item>
									 <item>  3  </item>
									 <item>  4  </item>	
									<item> 8  </item>								 									 
								</one-of>
							</rule>
					</grammar>
					<prompt bargein="true" timeout="1s">
						<foreach item="names" array="array1">
<!--							<mark nameexpr="movie_idx++"/>-->
							<log>	song is is    	<value expr="movie_idx"/>  	</log>
							<log>	names is is    	<value expr="names"/>  	</log>
							<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+names+'/lang'+names+'.wav'"/>
							<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+names+'/press_dtmf'+ ++movie_idx+'.wav'"/>			
						</foreach>							
					</prompt>
<!--					<prompt bargein="true" timeout="0s">
							<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+array1[movie_idx-1]+'/lang'+array1[movie_idx-1]+'.wav'"/>
							<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+array1[movie_idx-1]+'/press_dtmf'+movie_idx+'.wav'"/>			
					</prompt>-->

				<filled>
					<if cond="choice &lt; len+1">
							<assign name="lang" expr="array1[--choice]"/>
							<log expr="'selected language is '+lang"></log>
							
								<if cond="directaccess=='combo' &amp;&amp; lang!='06'">	
										<assign name="lang" expr="'01'"></assign>									
								</if>
							
							<if cond="usr_status=='-1'">
							    <prompt bargein="true" timeout="0s">
									<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/lang_thanks.wav'"/>
								</prompt>
								<goto next="Hungama_ModSub.vxml"/>
							<else/>
								<if cond="(DNIS=='590902' || DNIS=='590905') &amp;&amp; operator=='tatm'">
									<prompt bargein="true" timeout="0s">
										<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/lang_thanks.wav'"/>
									</prompt>
									<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RADIO_LANG_CHANGE_TOLL&amp;INTOKEN=3&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+lang+'&amp;INPARAM[2]='+DNIS"/>
									<goto next="Hungama_ModMainMenu.vxml"/>
								<else/>
							    		<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RADIO_LANG_CHANGE&amp;INTOKEN=2&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+lang"/>
							    	</if>
								<prompt bargein="true" timeout="0s">
									<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/lang_thanks_acc.wav'"/>
								</prompt>
								<goto next="Hungama_ModMainMenu.vxml"/>
							</if>
					</if>
				</filled>

			<noinput count="1">
				<prompt bargein="true" timeout="0s">
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/nodtmf.wav'"/>
				</prompt>
				<assign name="movie_idx" expr="0"/>
				<reprompt></reprompt>
			</noinput>
			<noinput count="2">
				<prompt bargein="true" timeout="0s">
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/nodtmf.wav'"/>
				</prompt>
				<assign name="movie_idx" expr="0"/>
				<reprompt></reprompt>
			</noinput>
			<noinput count="3">
				<if cond="usr_status=='-1'">
				    <assign name="lang" expr="array1[0]"/>
					<log expr="'selected language is '+lang"></log>			    
					<goto next="Hungama_ModSub.vxml"/>
				<else/>
					<goto next="Hungama_ModMainMenu.vxml"/>
				</if>
			</noinput>
			<nomatch count="1">
				<prompt bargein="true" timeout="0s">
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>
				</prompt>
				<assign name="movie_idx" expr="0"/>
				<reprompt></reprompt>
			</nomatch>
			<nomatch count="2">
				<prompt bargein="true" timeout="0s">
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>
				</prompt>
				<assign name="movie_idx" expr="0"/>
				<reprompt></reprompt>
			</nomatch>
			<nomatch count="3">
				<if cond="usr_status=='-1'">
				    <assign name="lang" expr="array1[0]"/>
					<log expr="'selected language is '+lang"></log>			    
					<goto next="Hungama_ModSub.vxml"/>
				<else/>
					<goto next="Hungama_ModMainMenu.vxml"/>
				</if>
			</nomatch>
<!--		<noinput>
				<script>movie_idx=movie_idx+1;</script>
				 <if cond="movie_idx &gt; maxlang">		   
					<script>	
					     movie_idx=1; 
					</script>
			        </if>		
				  <goto nextitem="choice"/>
		</noinput>
		<nomatch>				    
			<script>movie_idx=1;</script>          		     
			<prompt>						
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>		 
			</prompt>
			<goto nextitem="choice"/>	
		</nomatch>-->
	</field>
	</form>
	<form id="THREEDAYSTNB">
		<block>
			<assign name="var_wel" expr="getRandom(0,20)" />
			<if cond="var_wel=='1' || var_wel=='6' || var_wel=='11' || var_wel=='16'">
				<assign name="var_wel" expr="'1'" />
			<elseif cond="var_wel=='2' || var_wel=='7' || var_wel=='12' || var_wel=='17'"/>
				<assign name="var_wel" expr="'2'" />
			<elseif cond="var_wel=='3' || var_wel=='8' || var_wel=='13' || var_wel=='18'"/>
				<assign name="var_wel" expr="'3'" />				
			<elseif cond="var_wel=='4' || var_wel=='9' || var_wel=='14' || var_wel=='19'"/>
				<assign name="var_wel" expr="'4'" />				
			<else/>
				<assign name="var_wel" expr="'5'" />
			</if>			
			<if cond="lang=='03' || lang=='04'|| lang=='05'|| lang=='06'|| lang=='11'|| lang=='12'|| lang=='13'|| lang=='18'">
				<assign name="lang" expr="'01'"/>
			</if>			
		</block>
		<field name="choice">			
			<grammar mode="dtmf" root="start" version="1.0">
						<rule id="start" scope="public">
							<one-of>
								 <item>  1  </item>
							</one-of>
						</rule>
				</grammar>
				
			<prompt bargein="true" timeout="3s">						
				<!--<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/Sub_tnb.wav'"/>-->
				<!--<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/Sub_tnb_'+var_wel+'.wav'"/>-->
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/subintro_2015-2-12.wav'"/>
			</prompt>	

			<filled>
				<if cond="choice=='1'">
					<!--<assign name="firstconsent" expr="'Y'"/>
					<assign name="firstconsenttime" expr="start_time()"/>
					<assign name="sub_flag" expr="'true'"/>-->
					<if cond="operator=='tatm'">					
						<assign name="planid" expr="'2'"/>
						<assign name="Amt" expr="'14'"/>
						<assign name="serviceid" expr="'1001'"/> 
						<assign name="MOS" expr="'TIVR'"/>
					<elseif cond="operator=='tatc'"/>	
						<assign name="planid" expr="'26'"/>
						<assign name="Amt" expr="'14'"/>
						<assign name="serviceid" expr="'1601'"/>
						<assign name="MOS" expr="'TIVR'"/>
					</if>
					<goto next="Hungama_ModSub1.vxml#Doubleconfirm"></goto>
				</if>
			</filled>
		</field>
		<noinput count="1">
			<prompt bargein="true" timeout="0s">
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/nodtmf.wav'"/>
			</prompt>			
			<reprompt></reprompt>
		</noinput>
		<noinput count="2">
			<prompt bargein="true" timeout="0s">
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/nodtmf.wav'"/>
			</prompt>			
			<reprompt></reprompt>
		</noinput>
		<noinput count="3">
			<goto next="Hungama_ModEnd.vxml"/>
		</noinput>
		<nomatch count="1">
			<prompt bargein="true" timeout="0s">
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>
			</prompt>			
			<reprompt></reprompt>
		</nomatch>
		<nomatch count="2">
			<prompt bargein="true" timeout="0s">
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>
			</prompt>			
			<reprompt></reprompt>
		</nomatch>
		<nomatch count="3">
			<goto next="Hungama_ModEnd.vxml"/>
		</nomatch>
	</form>	
	
	<form id="LANGSEL1">
		<block>	
			
			<log>LANG SEL1</log>
			<assign name="program_name" expr="'Hungama_ModNewUser_LANGSEL1'"/>
			<if cond="circle=='del' &amp;&amp; operator=='tatc'">
				<goto next="#LANGSELDEL"></goto>
