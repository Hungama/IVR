			<prompt bargein="true" timeout="0s" cond="operator=='tatc' &amp;&amp; directaccess!='combo' &amp;&amp; directaccess!='SangamContest' &amp;&amp; circle!='kar'">
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/common/welcome_new_indicom_'+lang+'.wav'"/>			
			</prompt>
			
			<prompt bargein="true" timeout="0s" cond="operator=='tatc' &amp;&amp; directaccess!='combo' &amp;&amp; directaccess!='SangamContest' &amp;&amp; circle=='kar' &amp;&amp; (lang=='02' || lang=='10')">
				<!--<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/common/welcome_new_indicom_'+lang+'.wav'"/>-->			
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/welcome_kk_spl.wav'"/>
			</prompt>			
			
			<prompt bargein="true" timeout="0s" cond="operator=='tatm' &amp;&amp; directaccess!='combo' &amp;&amp; directaccess!='SangamContest' &amp;&amp; DNIS!='5909013' &amp;&amp; circle!='kar'">			
				<!--<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/common/welcome_new_docomo_'+lang+'.wav'"/>-->
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/cin_welcome_new_47.wav'"/>
				<!--<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/welcome_new_1.wav'"/> -->                        	
			</prompt>			

			<prompt bargein="true" timeout="0s" cond="operator=='tatm' &amp;&amp; directaccess!='combo' &amp;&amp; directaccess!='SangamContest' &amp;&amp; DNIS!='5909013' &amp;&amp; circle=='kar' &amp;&amp; (lang=='02' || lang=='10')">			
				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/welcome_kk_spl.wav'"/>                      	
			</prompt>		

			<prompt bargein="true" timeout="0s" cond="operator=='tatm' &amp;&amp; directaccess!='combo' &amp;&amp; directaccess=='SangamContest' &amp;&amp; DNIS!='5909013' &amp;&amp; circle!='kar'">			
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/welcome_new_1.wav'"/>
					<!--<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/common/welcome_new_docomo_'+lang+'.wav'"/>-->
			</prompt>

				<!--<if cond="circle!='mpd'  &amp;&amp; (realDNIS=='590907'  || realDNIS=='011590907' || realDNIS=='0111590907' )">-->
				<if cond="(circle=='raj' || circle=='del' || circle=='hpd' || circle=='hay'|| circle=='jnk' || circle=='mpd' || circle=='mum'  || circle=='upw') &amp;&amp; DNIS=='590902' &amp;&amp; DNIS=='590905' &amp;&amp; operator=='tatc'">
					<assign name="lang" expr="'01'"/>	
					<goto next="Hungama_ModSub.vxml"/>
				</if>				
				<if cond="(circle=='raj' || circle=='del' || circle=='hpd' || circle=='hay'|| circle=='jnk' || circle=='mpd' || circle=='mum') &amp;&amp; DNIS!='590902' &amp;&amp; DNIS!='590905'">
					<assign name="lang" expr="'01'"/>	
					<goto next="Hungama_ModSub.vxml"/>
				</if>

				<if cond="circle=='mpd' &amp;&amp; usr_status=='-1' &amp;&amp; (operator=='tatm' || operator=='virm')">
					<assign name="MP_HANGUP" expr="'true'"/>				
					<goto next="Hungama_ModSub.vxml"/>
                        	<elseif cond="usr_status=='-1' &amp;&amp; DNIS=='59090' &amp;&amp; operator=='tatc' &amp;&amp;  (circle=='nes' || circle=='asm')"/>					
					<goto next="Hungama_ModSub.vxml"/>			
				<else/>
