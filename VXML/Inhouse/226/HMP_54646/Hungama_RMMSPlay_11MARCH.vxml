				</script>
					<if cond="celeb_on_time=='2'">
							<prompt bargein="true">	
								<audio expr="promptpath+'/'+lang+'/chat_today.wav'"/>
							</prompt>
							<goto next="#userstatus"/>
					<elseif cond="celeb_on_time=='3'"/>
						<!--<prompt bargein="true">	
