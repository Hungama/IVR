<?xml version="1.0" encoding="UTF-8"?><vxml version="2.0" application="Hungama_ERoot.vxml"	xmlns="http://www.w3.org/2001/vxml"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">	<meta name="Retailer.vxml" content="" />	<meta name="Author" content="" />	<meta name="Date" content="" />	<meta name="Description" content="" />	<meta name="Support" content="" />		<var name="number" expr="'0'"/>	<var name ="MOB" expr =""/>
<form id="RetailUser">	<block>		<log expr="'############ In Retailer Menu #############'"></log>	</block>		<field name="mobilenumber" type="digits?length=10">					    		<prompt bargein="true" timeout="4s">				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/Welcome_mdn_Retailer.wav'"/>							</prompt>			<filled>				<script>						mobilenumber= mobilenumber.replace(/\s+/g,'');						number = mobilenumber.length;						MOB=mobilenumber;				</script>				<if cond="number=='10'">					<goto next="#confirm"/>				<else/>					<prompt bargein="true" timeout="0s">						<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>					</prompt>						<reprompt/>				</if>			</filled>			<noinput count="1">					<prompt bargein="true" timeout="0s">					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/nodtmf.wav'"/>					</prompt>								<reprompt></reprompt>			</noinput>			<noinput count="2">					<prompt bargein="true" timeout="0s">					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/nodtmf.wav'"/>					</prompt>								<reprompt></reprompt>			</noinput>			<noinput count="3">					<goto next="Hungama_ModEnd.vxml"></goto>			</noinput>			<nomatch count="1">					<prompt bargein="true" timeout="0s">					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>					</prompt>								<reprompt></reprompt>			</nomatch>					<nomatch count="2">					<prompt bargein="true" timeout="0s">					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>					</prompt>								<reprompt></reprompt>			</nomatch>			<nomatch count="3">					<goto next="Hungama_ModEnd.vxml"></goto>			</nomatch>					</field></form><form id="confirm">	<field name="choice">						<grammar mode="dtmf" root="start1" version="1.0">			<rule id="start1" scope="public">				<one-of>				 	<item>  5  </item>												</one-of>			</rule>		</grammar>					<prompt bargein="true" timeout="4s">		<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/confirmation.wav'"/>						</prompt>			<filled>			<if cond="choice=='5'">				<prompt bargein="true" timeout="0s">					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/Thanks_retailer.wav'"/>								</prompt>					<if cond="operator=='tatm'">						<assign name="planid" expr="'3'"/>						<assign name="Amt" expr="'60'"/>	                    <assign name="serviceid" expr="'1001'"/> 				<elseif cond="operator=='tatc'"/>						<assign name="planid" expr="'27'"/>						<assign name="Amt" expr="'60'"/>						<assign name="serviceid" expr="'1601'"/>				</if>				<assign name="lang" expr="'01'"/>				<assign name="MOS" expr="'IVR'"/>				<!--<assign name="MOB"> <value expr="mobilenumber"/></assign>-->												<log expr="'mobilenumber @@@@@@@@@= '+MOB"></log>				<log expr="'Amt @@@@@@@@@= '+Amt"></log>				<log expr="'DNIS @@@@@@@@@= '+DNIS"></log>				<log expr="'serviceid @@@@@@@@@= '+serviceid"></log>				<log expr="'planid @@@@@@@@@= '+planid"></log>				<!--RADIO_SUB(N_ANI,IN_LANG,IN_MOD,IN_DNIS,IN_AMNT,IN_SID,IN_PID);-->	      		<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RADIO_SUB&amp;INTOKEN=7&amp;OUTTOKEN=0&amp;INPARAM[0]='+MOB+'&amp;INPARAM[1]='+lang+'&amp;INPARAM[2]='+MOS+'&amp;INPARAM[3]='+DNIS+'&amp;INPARAM[4]='+Amt+'&amp;INPARAM[5]='+serviceid+'&amp;INPARAM[6]='+planid"/>				<goto next="Hungama_ModEnd.vxml"></goto>			</if>						</filled>	</field>		<noinput count="1">				<prompt bargein="true" timeout="0s">				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/nodtmf.wav'"/>				</prompt>							<reprompt></reprompt>		</noinput>		<noinput count="2">				<prompt bargein="true" timeout="0s">				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/nodtmf.wav'"/>				</prompt>							<reprompt></reprompt>		</noinput>		<noinput count="3">				<goto next="Hungama_ModEnd.vxml"></goto>		</noinput>		<nomatch count="1">				<prompt bargein="true" timeout="0s">				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>				</prompt>							<reprompt></reprompt>		</nomatch>		<nomatch count="2">				<prompt bargein="true" timeout="0s">				<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/invalidoption.wav'"/>				</prompt>							<reprompt></reprompt>		</nomatch>		<nomatch count="3">				<goto next="Hungama_ModEnd.vxml"></goto>		</nomatch></form></vxml>