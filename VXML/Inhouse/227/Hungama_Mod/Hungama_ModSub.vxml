					<log expr="'tnbcheck here @@@@@@@@@@@@@@@@@@@@ =' +tnbcheck"/>		        
					<log expr="'trybuy here @@@@@@@@@@@@@@@@@@@@ =' +trybuy"/>

	<elseif cond="DNIS=='5909066'"/>
