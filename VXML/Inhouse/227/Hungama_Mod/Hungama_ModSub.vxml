	<form id="BillStatusChk">
		<block>


				<if cond="AudioCinema=='yes'">
						<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RADIO_lastusercat&amp;INTOKEN=2&amp;OUTTOKEN=1&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]=1'"/>
					<script>
						 <![CDATA[
					   for(j=0; j< out_string.length;  j++)
						  {  	  
						       if(j==0)
						       	LastUserCat= out_string[j];
						  }

					]]>
			
					</script>


						<assign name="LastUserCat" expr="'AUDIOCINEMA'"/>

						
				</if>



			<!-- SUB REQUEST STATUS -->

				<script>
					indexCounter=indexCounter+1;
				</script>

			<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=RADIO_QUERY&amp;INTOKEN=2&amp;OUTTOKEN=1&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]=1'"/>
			<script>
			 <![CDATA[
					   for(j=0; j< out_string.length;  j++)
						  {  	  
						       if(j==0)
						       	usr_status = out_string[j];
						       if(j==1)
						       	lang = out_string[j];
						       if(j==2)
						       	bal = out_string[j];
						  }
				]]>
			
			</script>
			<log expr="'usr_status @@@@@@@@@ '+usr_status"></log>		
			<log expr="'lang  @@@@@@@@@ '+lang"></log>
			<log expr="'bal @@@@@@@@@ '+bal"></log>	

			<if cond="usr_status=='-1'">
				<prompt bargein="true" timeout="0s">
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/delay_charging.wav'"/>
				</prompt>
			
				<log>indexCounter:<value expr="indexCounter"/></log>

				<if cond="indexCounter&lt;4">
				    	<goto next="#BillStatusChk"/>	
				</if>
				<goto next="Hungama_ModEnd.vxml"/>
					
			<elseif cond="usr_status=='1'"/>
				<script>bal=bal*60;</script>
				<prompt bargein="true" timeout="0s" cond="directaccess!='combo'">
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/thank_sub.wav'"/>
				</prompt>
				<prompt bargein="true" timeout="0s" cond="directaccess=='combo'">
					<audio expr="'http://'+PROMPTS_CONTENT+'/'+Prompts_Path+'/'+lang+'/thank_sub_combo.wav'"/>
				</prompt>

				<if cond="AudioCinema=='yes'">
						<goto next="Hungama_ModMainMenu.vxml"/>
				</if>

					<if cond="directaccess=='MM' &amp;&amp; DNIS!='5909013'">
						<assign name="DNIS" expr="DNISset"/>
						<goto next="Hungama_MM_MENU.vxml"></goto>
					<elseif cond="directaccess=='MM'  &amp;&amp; DNIS=='5909013'"/>
						<goto next="Hungama_MM_MENU.vxml"></goto>
					<elseif cond="directaccess=='combo'"/>	
						<goto next="Hungama_combo_menu.vxml"></goto>					
					<else/>
						<goto next="Hungama_ModMainMenu.vxml"></goto>
	            	</if>
			<else/>
				<goto next="Hungama_ModEnd.vxml"/>
			</if>
		</block>
	</form>

