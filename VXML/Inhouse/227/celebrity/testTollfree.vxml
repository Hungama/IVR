<vxml version="2.1" application="FMJRoot.vxml">              <property name="documentmaxstale" value="0s"></property>	<property name="documentmaxage" value="0s"></property>	   <var name="len" expr="''"/>	   <var name="Digit1st" expr="0"/>   <var name="Digit2nd" expr="0"/>   <var name="Digit3rd" expr="0"/>   <var name="Digit4th" expr="0"/>   <var name="PlayOnce" expr="true"/>		   <var name="names"/>        <script>		var favSongs=new Object();		var out_string=new Object();	</script>	   <!--<form id="SubIntro">		<block>			<prompt bargein="true" timeout="0s">				    <audio expr="'http://'+CDSIP+CDSPATH+'promptFiles/'+LANG_DESCRIPTION+'/'+'subscriptionpack_details.wav'"/>			   	      </prompt>			<goto next="#SubOption"/>		</block>	</form>-->			<form id="SubOption">		<field name="choice">			<grammar mode="dtmf" root="start" version="1.0">						<rule id="start" scope="public">							<one-of>								 <item>  1  </item>							</one-of>						</rule>				</grammar>									<prompt bargein="true" timeout="3s">	               			<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'sub_intro.wav'"/>		            		</prompt>			<filled>				<if cond="choice=='1'">					<goto next="#double_conf"></goto>					</if>			</filled>						<noinput timeout="0s">			     <audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'nodtmf.wav'"/>                                 <reprompt/>              </noinput>                     	                     <nomatch>                <audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'invalidoption.wav'"/>		                <reprompt/>             </nomatch>              		</field>	</form>		<form id="double_conf">   		<field name="choice">			<grammar mode="dtmf" root="start" version="1.0">               <rule id="start" scope="public">                 <one-of>				 				  <item>5</item>				  <item>2</item>				  			  		      				  			  		                      </one-of>              </rule>            </grammar>	     	<prompt bargein="true" timeout="3s">	     		<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'conf_subs.wav'"/>			     	</prompt> 				             <filled> 				<if cond="choice=='5'">					<goto next="#SubRequest"/>				<else/>					<goto next="starclub_End.vxml"/>            	</if>            </filled>            </field>            <noinput count="1">            	<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'nodtmf.wav'"/>            	<reprompt/>            </noinput>            <noinput count="2">	            	<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'nodtmf.wav'"/>            	<reprompt/>            </noinput>            <noinput count="3">            	<goto next="starclub_End.vxml"/>            </noinput>			<nomatch count="1">				 <audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'invalidoption.wav'"/>				<reprompt/>			</nomatch>			<nomatch count="2">				 <audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'invalidoption.wav'"/>				<reprompt/>			</nomatch>			<nomatch count="3">				<goto next="starclub_End.vxml"/>			</nomatch>   </form>			<form id="SubRequest">		<block>			<!-- SUB REQUEST -->			<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=JBOX_SUB&amp;INTOKEN=7&amp;OUTTOKEN=0&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]='+lang+'&amp;INPARAM[2]=IVR&amp;INPARAM[3]='+DNIS+'&amp;INPARAM[4]=30&amp;INPARAM[5]='+serviceid+'&amp;INPARAM[6]='+planid"/>			<goto next="#BillStatusChk"/>			</block>	</form>	<form id="BillStatusChk">			<block>						 <prompt bargein="false">					<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'charging_wait.wav'"/>					<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'wait_music.wav'"/>			</prompt> 		     			<!-- SUB REQUEST STATUS -->						<script srcexpr="'http://'+IP+'/'+servlet_path+'/'+dbservlet+'?PROCEDURE=JBOX_QUERY&amp;INTOKEN=2&amp;OUTTOKEN=1&amp;INPARAM[0]='+ANI+'&amp;INPARAM[1]=1'"/>			<script>			 <![CDATA[					   for(j=0; j< out_string.length;  j++)						 {  	  						       if(j==0)						       	usr_status = out_string[j];						       if(j==1)						       	lang = out_string[j];						       if(j==2)						       	balance = out_string[j];						  }					]]>						</script>						<if cond="usr_status=='-1'">				<prompt bargein="true" timeout="0s">					<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'delay_charging.wav'"/>				</prompt>				<exit/>			<elseif cond="usr_status=='1'"/>									<script> 					 					    balance=balance*60;					    timerflag=1;      					 </script>				 					<!-- <log expr="'@@@@@@ user start balance is '+balance"/>						 <log expr="'@@@@@@ start_BalenceMinute is '+start_BalenceMinute"/>	-->					 <if cond="timerflag=='1'">                       						   					   		<script>					     		total_used_bal=chkBalenceMinute(start_BalenceMinute);                         		curbalance=balance-total_used_bal;  						  					   		</script>					   	<!--	<log expr="'@@@@@@ used balance is '+total_used_bal"/>					   		<log expr="'@@@@@@ left balance is '+curbalance"/> -->                         	<if cond="curbalance &lt;= '0'">                                       	<goto next="TopupRenew.vxml"/>                        	</if>                         </if>    					 <prompt bargein="true" timeout="0s">	               			<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'thank_sub.wav'"/>		            		</prompt>				 	     			 	     <goto next="#hungamasubcatMenu"/>				<elseif cond="usr_status == '3'"/>						     <goto nextitem="hungamasublowbalintro"/>			  	 				  				  				     		    <else/>						  <prompt bargein="true" timeout="0s">					<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'delay_charging.wav'"/>				</prompt>				<!--<exit/>-->				<exit/>   			    			</if>		</block>				<field name="hungamasublowbalintro" cond="usr_status=='3'">               		    	 				 <grammar mode="dtmf" root="start" version="1.0">                   <rule id="start" scope="public">                     <one-of>				          <item>1</item>				          <item>2</item>	                          <item>3</item>	                          <item>4</item>	                          <item>5</item>	                          <item>6</item>	                          <item>7</item>	                          <item>8</item>	                          <item>9</item>	                          <item>0</item>	               			 				          <item>#</item>				          <item>*</item>			                          </one-of>                  </rule>               </grammar> 				   	               <prompt bargein="true" timeout="0s">				    <audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'subscription-notsuccessful.wav'"/>			   	      </prompt> 				               <filled>                                               <clear namelist="hungamasublowbalintro"/> 				   <script>				      					 					  total_used_bal=chkBalenceMinute(start_BalenceMinute);  					  curbalance=balance-total_used_bal;  					 			       </script>				  				  <!-- <log expr="'@@@@@@ used balance is '+total_used_bal"/>				   <log expr="'@@@@@@ left balance is '+curbalance"/>    -->              				  				  				   		                  			     			 	   <throw event="connection.disconnect.hangup"/>	                 </filled>			  			  <noinput>		  	               <clear namelist="hungamasublowbalintro"/> 				   <script>				      				 					  total_used_bal=chkBalenceMinute(start_BalenceMinute);  					  curbalance=balance-total_used_bal;  					 			       </script>				  				  <!-- <log expr="'@@@@@@ used balance is '+total_used_bal"/>				   <log expr="'@@@@@@ left balance is '+curbalance"/>   -->               				  				  				   	 	                  			 	   <throw event="connection.disconnect.hangup"/>	                 </noinput>	        </field>        	</form>	          <form id="hungamasubcatMenu">                <block>		 				  		<!-- <log expr="'@@@@@@ call start_BalenceMinute is '+start_BalenceMinute"/> -->		 		<!-- <log> cat order servlet </log>			<log expr="'operator : ' + operator"/>  			<log expr="'circle: ' + circle"/>	-->												   			<!--<script srcexpr="'http://'+IP+'/hungama/ar_ReadConfigSong?ConfigPath=starclubconfig/'+operator+'/'+circle+'/mainmenuorder.cfg'"/> -->				<script srcexpr="'http://'+IP+'/hungama/ar_ReadConfigSong?ConfigPath=starclubconfig/mainmenuorder.cfg'"/>			<script>					    	array1.length=0;			</script>			<assign name="len" expr="favSongs.length" />			<assign name="i" expr="0" />		<!--	<log>previous Array[1]  is	<value expr="array1[0]"/>    </log> -->			<script>				 <![CDATA[					   for(j=0; j<len ;  j++)					   {  	  								array1[j] = favSongs[j];						   					   }				]]>					</script> 	       					       		<!--	<log>Length  is	<value expr="len"/>    </log>			<log>Array[1]  is	<value expr="array1[0]"/>    </log> -->       		</block>				<!--<log expr="'welcome to the hungama sub category main menu '"/>                <log expr="' Server ip is :' + IP"/>	    <log expr="'Ani IS : ' + ANI"/> 			<log expr="'DNIS IS : ' + DNIS"/> 	-->				 	    <field name="mnuopt">			 <grammar mode="dtmf" root="start" version="1.0">               <rule id="start" scope="public">                  <one-of>				 				   				    <item>2</item>				    <item>3</item>				  			 				    <item>4</item>				    <item>5</item>				    <item>6</item>					    <item>9</item>					  		                      </one-of>              </rule>            </grammar>					   			<prompt bargein="true" timeout="3s">	           	<audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'select_menu2.wav'"/>            </prompt>		    						<filled>								<if cond="mnuopt=='9'">					    <clear namelist="mnuopt"/> 					<if cond="timerflag=='1'">					   <script>					     total_used_bal=chkBalenceMinute(start_BalenceMinute);                         curbalance=balance-total_used_bal;  						  					   </script>					<!--   <log expr="'@@@@@@ used balance is '+total_used_bal"/>					   <log expr="'@@@@@@ left balance is '+curbalance"/> -->                         <if cond="curbalance &lt;= '0'">                                       <goto next="TopupRenew.vxml"/>                        </if>     					 </if>				    <goto next="actmanager.vxml"/>						</if>								<if cond="mnuopt &lt; len+1">					<if cond="mnuopt=='1'">						<assign name="usropt" expr="2"/>						<assign name="cat" expr="array1[1]"/>					<else/> 						<assign name="usropt" expr="mnuopt"/>						<assign name="cat" expr="array1[--mnuopt]"/>					</if>									<!--	<log expr="'selected cat is '+cat"></log> -->					<assign name="playconfig" expr="cat"></assign>				<!--	<log expr="'User Input value==============>'+usropt  +mnuopt"/> -->					<if cond="timerflag=='1'">					   <script>					     total_used_bal=chkBalenceMinute(start_BalenceMinute);                         curbalance=balance-total_used_bal;  						  					   </script>					<!--   <log expr="'@@@@@@ Smita start_BalenceMinute is '+start_BalenceMinute"/>					   <log expr="'@@@@@@ Smita used balance is '+total_used_bal"/>					   <log expr="'@@@@@@ Smita left balance is '+curbalance"/> -->                         <if cond="curbalance &lt;= '0'">                                       <goto next="TopupRenew.vxml"/>                        </if>     					 </if>						<goto next="playContent.vxml"/>									</if>						</filled>											 <noinput timeout="3s">			     <audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'nodtmf.wav'"/>                                 <reprompt/>              </noinput>                                 <noinput count="3">		                  <assign name="usropt" expr="2"/>					<assign name="cat" expr="array1[1]"/>									<!--	<log expr="'default cat is '+cat"></log> -->					<assign name="playconfig" expr="cat"></assign>										<if cond="timerflag=='1'">					   <script>					     total_used_bal=chkBalenceMinute(start_BalenceMinute);                         curbalance=balance-total_used_bal;  						  					   </script>					 <!--  <log expr="'@@@@@@ Smita start_BalenceMinute is '+start_BalenceMinute"/>					   <log expr="'@@@@@@ Smita used balance is '+total_used_bal"/>					   <log expr="'@@@@@@ Smita left balance is '+curbalance"/> -->                         <if cond="curbalance &lt;= '0'">                                       <goto next="TopupRenew.vxml"/>                        </if>     					 </if>											<goto next="playContent.vxml"/>							              </noinput>              	                     <nomatch>                <audio expr="Prompt_Path+LANG_DESCRIPTION+'/'+'invalidoption.wav'"/>		                <reprompt/>             </nomatch>                           <nomatch count="3">	             <assign name="usropt" expr="2"/>					<assign name="cat" expr="array1[1]"/>									<!--	<log expr="'default cat is '+cat"></log> -->					<assign name="playconfig" expr="cat"></assign>										<if cond="timerflag=='1'">					   <script>					     total_used_bal=chkBalenceMinute(start_BalenceMinute);                         curbalance=balance-total_used_bal;  						  					   </script>					<!--   <log expr="'@@@@@@ Smita start_BalenceMinute is '+start_BalenceMinute"/>					   <log expr="'@@@@@@ Smita used balance is '+total_used_bal"/>					   <log expr="'@@@@@@ Smita left balance is '+curbalance"/> -->                         <if cond="curbalance &lt;= '0'">                                       <goto next="TopupRenew.vxml"/>                        </if>     					 </if>											<goto next="playContent.vxml"/>					         </nomatch> 	                 							  </field>	   		    </form> 		</vxml>   